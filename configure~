$as_echo "$as_me:   * removed option --enable-gpu" >&6;}
  { $as_echo "$as_me:${as_lineno-$LINENO}:     >>> use --with-gpu instead" >&5
$as_echo "$as_me:     >>> use --with-gpu instead" >&6;}
  if test "${enable_gpu}" != ""; then
