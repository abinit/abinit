ac_compiler_gnu=$ac_cv_c_compiler_gnu


fi
{ $as_echo "$as_me:${as_lineno-$LINENO}: result: $ax_cv_f90_modext" >&5
$as_echo "$ax_cv_f90_modext" >&6; }
  if test "${ax_cv_f90_modext}" != ""; then
