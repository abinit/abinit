$as_echo "$as_me:   * MPI enabled (required)                       : ${abi_mpi_enable}" >&6;}
  { $as_echo "$as_me:${as_lineno-$LINENO}:   * MPI C compiler is set (required)             : ${sd_mpi_cc_set}" >&5
$as_echo "$as_me:   * MPI C compiler is set (required)             : ${sd_mpi_cc_set}" >&6;}
  { $as_echo "$as_me:${as_lineno-$LINENO}:   * MPI C compiler works (required)              : ${sd_mpi_cc_ok}" >&5
$as_echo "$as_me:   * MPI C compiler works (required)              : ${sd_mpi_cc_ok}" >&6;}
  { $as_echo "$as_me:${as_lineno-$LINENO}:   * MPI Fortran compiler is set (required)       : ${sd_mpi_fc_set}" >&5
$as_echo "$as_me:   * MPI Fortran compiler is set (required)       : ${sd_mpi_fc_set}" >&6;}
  { $as_echo "$as_me:${as_lineno-$LINENO}:   * MPI Fortran compiler works (required)        : ${sd_mpi_fc_ok}" >&5
$as_echo "$as_me:   * MPI Fortran compiler works (required)        : ${sd_mpi_fc_ok}" >&6;}
  { $as_echo "$as_me:${as_lineno-$LINENO}:   * MPI environment usable (required)            : ${abi_mpi_ok}" >&5
$as_echo "$as_me:   * MPI environment usable (required)            : ${abi_mpi_ok}" >&6;}
  { $as_echo "$as_me:${as_lineno-$LINENO}:   * MPI C++ compiler is set (optional)           : ${sd_mpi_cxx_set}" >&5
$as_echo "$as_me:   * MPI C++ compiler is set (optional)           : ${sd_mpi_cxx_set}" >&6;}
  { $as_echo "$as_me:${as_lineno-$LINENO}:   * MPI C++ compiler works (optional)            : ${sd_mpi_cxx_ok}" >&5
$as_echo "$as_me:   * MPI C++ compiler works (optional)            : ${sd_mpi_cxx_ok}" >&6;}
  { $as_echo "$as_me:${as_lineno-$LINENO}:   * MPI-in-place enabled (optional)              : ${abi_mpi_inplace_enable}" >&5
$as_echo "$as_me:   * MPI-in-place enabled (optional)              : ${abi_mpi_inplace_enable}" >&6;}
  { $as_echo "$as_me:${as_lineno-$LINENO}:   * MPI-IO enabled (optional)                    : ${abi_mpi_io_enable}" >&5
$as_echo "$as_me:   * MPI-IO enabled (optional)                    : ${abi_mpi_io_enable}" >&6;}
  { $as_echo "$as_me:${as_lineno-$LINENO}:   * MPI configuration type (computed)            : ${abi_mpi_init}" >&5
$as_echo "$as_me:   * MPI configuration type (computed)            : ${abi_mpi_init}" >&6;}
  { $as_echo "$as_me:${as_lineno-$LINENO}:   * MPI Fortran level supported (detected)       : ${abi_mpi_fc_level}" >&5
$as_echo "$as_me:   * MPI Fortran level supported (detected)       : ${abi_mpi_fc_level}" >&6;}
  { $as_echo "$as_me:${as_lineno-$LINENO}:   * MPI_Get_library_version available (detected) : ${abi_mpi_get_library_version}" >&5
$as_echo "$as_me:   * MPI_Get_library_version available (detected) : ${abi_mpi_get_library_version}" >&6;}
  { $as_echo "$as_me:${as_lineno-$LINENO}: " >&5
$as_echo "$as_me: " >&6;}
  { $as_echo "$as_me:${as_lineno-$LINENO}: All required parameters must be set to 'yes'." >&5
$as_echo "$as_me: All required parameters must be set to 'yes'." >&6;}
  { $as_echo "$as_me:${as_lineno-$LINENO}: If not, the configuration and/or the build with" >&5
$as_echo "$as_me: If not, the configuration and/or the build with" >&6;}
  { $as_echo "$as_me:${as_lineno-$LINENO}: MPI support will very likely fail." >&5
$as_echo "$as_me: MPI support will very likely fail." >&6;}
  { $as_echo "$as_me:${as_lineno-$LINENO}: " >&5
$as_echo "$as_me: " >&6;}

fi

# GPU support

  # Initial setup
  abi_gpu_serial="no"
  abi_gpu_mpi="no"
  abi_gpu_precision=`echo "${abi_gpu_flavor}" | cut -d- -f2`
  test "${abi_gpu_precision}" = "" && abi_gpu_precision="single"

  # Display user requests
  { $as_echo "$as_me:${as_lineno-$LINENO}: checking whether to activate GPU support" >&5
$as_echo_n "checking whether to activate GPU support... " >&6; }
  { $as_echo "$as_me:${as_lineno-$LINENO}: result: ${abi_gpu_enable}" >&5
$as_echo "${abi_gpu_enable}" >&6; }

  # Look for GPU libraries
  if test "${abi_gpu_enable}" = "yes"; then

    # Check whether we have a working gpu environment
    { $as_echo "$as_me:${as_lineno-$LINENO}: checking for the requested GPU support" >&5
$as_echo_n "checking for the requested GPU support... " >&6; }
    { $as_echo "$as_me:${as_lineno-$LINENO}: result: ${abi_gpu_flavor}" >&5
$as_echo "${abi_gpu_flavor}" >&6; }

    case "${abi_gpu_flavor}" in

      cuda*)

  # Init
  abi_gpu_cuda_serial="no"
  abi_gpu_cuda_mpi="no"
  abi_gpu_cuda_old="unknown"

  # Display variables
  { $as_echo "$as_me:${as_lineno-$LINENO}: Cuda incs: ${abi_gpu_cuda_incs}" >&5
$as_echo "$as_me: Cuda incs: ${abi_gpu_cuda_incs}" >&6;}
  { $as_echo "$as_me:${as_lineno-$LINENO}: Cuda libs: ${abi_gpu_cuda_libs}" >&5
$as_echo "$as_me: Cuda libs: ${abi_gpu_cuda_libs}" >&6;}

  # Prepare environment


    abi_env_ABI_CPPFLAGS="${ABI_CPPFLAGS}"

    abi_env_AR="${AR}"

    abi_env_ARFLAGS="${ARFLAGS}"

    abi_env_ARFLAGS_CMD="${ARFLAGS_CMD}"

    abi_env_ARFLAGS_DEBUG="${ARFLAGS_DEBUG}"

    abi_env_ARFLAGS_EXTRA="${ARFLAGS_EXTRA}"

