$as_echo "$as_me:   * removed option --enable-fallbacks" >&6;}
  { $as_echo "$as_me:${as_lineno-$LINENO}:     >>> use --with-fallbacks instead" >&5
$as_echo "$as_me:     >>> use --with-fallbacks instead" >&6;}
