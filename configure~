    NM="${abi_env_NM}"

    NVCC="${abi_env_NVCC}"

    NVCC_CFLAGS="${abi_env_NVCC_CFLAGS}"

    NVCC_CPPFLAGS="${abi_env_NVCC_CPPFLAGS}"

    NVCC_LDFLAGS="${abi_env_NVCC_LDFLAGS}"

    NVCC_LIBS="${abi_env_NVCC_LIBS}"

    PYFLAGS="${abi_env_PYFLAGS}"

    PYTHON_CPPFLAGS="${abi_env_PYTHON_CPPFLAGS}"

