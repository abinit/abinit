              { { $as_echo "$as_me:${as_lineno-$LINENO}: error: in \`$ac_pwd':" >&5
$as_echo "$as_me: error: in \`$ac_pwd':" >&2;}
as_fn_error $? "invalid NetCDF configuration
See \`config.log' for more details" "$LINENO" 5; }
        else
              { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: invalid NetCDF configuration" >&5
$as_echo "$as_me: WARNING: invalid NetCDF configuration" >&2;}
        fi
      fi
    fi
  else
    sd_netcdf_enable="no"
    sd_netcdf_cppflags=""
    sd_netcdf_cflags=""
    sd_netcdf_cxxflags=""
    sd_netcdf_fcflags=""
    sd_netcdf_ldflags=""
    sd_netcdf_libs=""
  fi


{ $as_echo "$as_me:${as_lineno-$LINENO}: " >&5
$as_echo "$as_me: " >&6;}
{ $as_echo "$as_me:${as_lineno-$LINENO}: dumping NetCDF parameters for diagnostics" >&5
$as_echo "$as_me: dumping NetCDF parameters for diagnostics" >&6;}
{ $as_echo "$as_me:${as_lineno-$LINENO}: -----------------------------------------" >&5
$as_echo "$as_me: -----------------------------------------" >&6;}
{ $as_echo "$as_me:${as_lineno-$LINENO}: " >&5
$as_echo "$as_me: " >&6;}
{ $as_echo "$as_me:${as_lineno-$LINENO}:    * sd_netcdf_ok = ${sd_netcdf_ok}" >&5
$as_echo "$as_me:    * sd_netcdf_ok = ${sd_netcdf_ok}" >&6;}
{ $as_echo "$as_me:${as_lineno-$LINENO}:    * sd_netcdf_mpi_ok = ${sd_netcdf_mpi_ok}" >&5
$as_echo "$as_me:    * sd_netcdf_mpi_ok = ${sd_netcdf_mpi_ok}" >&6;}
{ $as_echo "$as_me:${as_lineno-$LINENO}:    * sd_netcdf_enable = ${sd_netcdf_enable}" >&5
$as_echo "$as_me:    * sd_netcdf_enable = ${sd_netcdf_enable}" >&6;}
{ $as_echo "$as_me:${as_lineno-$LINENO}:    * sd_netcdf_prefix = ${sd_netcdf_prefix}" >&5
$as_echo "$as_me:    * sd_netcdf_prefix = ${sd_netcdf_prefix}" >&6;}
{ $as_echo "$as_me:${as_lineno-$LINENO}: " >&5
$as_echo "$as_me: " >&6;}


# NetCDF library support (must be done after HDF5)

  # Display configuration

  { $as_echo "$as_me:${as_lineno-$LINENO}: checking whether to enable the NetCDF Fortran interface" >&5
$as_echo_n "checking whether to enable the NetCDF Fortran interface... " >&6; }
  { $as_echo "$as_me:${as_lineno-$LINENO}: result: ${sd_netcdf_fortran_enable}" >&5
$as_echo "${sd_netcdf_fortran_enable}" >&6; }
  if test "${sd_netcdf_fortran_enable}" != "no"; then
    { $as_echo "$as_me:${as_lineno-$LINENO}: checking how the NetCDF Fortran interface parameters have been set" >&5
$as_echo_n "checking how the NetCDF Fortran interface parameters have been set... " >&6; }
    { $as_echo "$as_me:${as_lineno-$LINENO}: result: ${sd_netcdf_fortran_init}" >&5
$as_echo "${sd_netcdf_fortran_init}" >&6; }
    { $as_echo "$as_me:${as_lineno-$LINENO}: checking for the NetCDF Fortran interface C preprocessing flags" >&5
$as_echo_n "checking for the NetCDF Fortran interface C preprocessing flags... " >&6; }
    if test "${sd_netcdf_fortran_cppflags}" = ""; then
      { $as_echo "$as_me:${as_lineno-$LINENO}: result: none" >&5
