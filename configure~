    abi_env_CC_LIBS_DEBUG="${CC_LIBS_DEBUG}"

    abi_env_CC_LIBS_EXTRA="${CC_LIBS_EXTRA}"

    abi_env_CC_LIBS_GPU="${CC_LIBS_GPU}"

    abi_env_CC_LIBS_HINTS="${CC_LIBS_HINTS}"

