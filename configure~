  if test "${sd_pfft_enable}" != "no"; then
    { $as_echo "$as_me:${as_lineno-$LINENO}: checking how PFFT parameters have been set" >&5
$as_echo_n "checking how PFFT parameters have been set... " >&6; }
    { $as_echo "$as_me:${as_lineno-$LINENO}: result: ${sd_pfft_init}" >&5
$as_echo "${sd_pfft_init}" >&6; }
    { $as_echo "$as_me:${as_lineno-$LINENO}: checking for PFFT C preprocessing flags" >&5
$as_echo_n "checking for PFFT C preprocessing flags... " >&6; }
    if test "${sd_pfft_cppflags}" = ""; then
      { $as_echo "$as_me:${as_lineno-$LINENO}: result: none" >&5
$as_echo "none" >&6; }
    else
      { $as_echo "$as_me:${as_lineno-$LINENO}: result: ${sd_pfft_cppflags}" >&5
$as_echo "${sd_pfft_cppflags}" >&6; }
    fi
    { $as_echo "$as_me:${as_lineno-$LINENO}: checking for PFFT C flags" >&5
$as_echo_n "checking for PFFT C flags... " >&6; }
    if test "${sd_pfft_cflags}" = ""; then
      { $as_echo "$as_me:${as_lineno-$LINENO}: result: none" >&5
$as_echo "none" >&6; }
    else
      { $as_echo "$as_me:${as_lineno-$LINENO}: result: ${sd_pfft_cflags}" >&5
$as_echo "${sd_pfft_cflags}" >&6; }
    fi
    { $as_echo "$as_me:${as_lineno-$LINENO}: checking for PFFT linker flags" >&5
$as_echo_n "checking for PFFT linker flags... " >&6; }
    if test "${sd_pfft_ldflags}" = ""; then
      { $as_echo "$as_me:${as_lineno-$LINENO}: result: none" >&5
$as_echo "none" >&6; }
    else
      { $as_echo "$as_me:${as_lineno-$LINENO}: result: ${sd_pfft_ldflags}" >&5
$as_echo "${sd_pfft_ldflags}" >&6; }
    fi
    { $as_echo "$as_me:${as_lineno-$LINENO}: checking for PFFT library flags" >&5
$as_echo_n "checking for PFFT library flags... " >&6; }
    if test "${sd_pfft_libs}" = ""; then
      { $as_echo "$as_me:${as_lineno-$LINENO}: result: none" >&5
$as_echo "none" >&6; }
    else
      { $as_echo "$as_me:${as_lineno-$LINENO}: result: ${sd_pfft_libs}" >&5
$as_echo "${sd_pfft_libs}" >&6; }
    fi
  fi


  # Check whether we can compile and link a simple program
  # and update build flags if successful
  if test "${sd_pfft_enable}" = "auto" -o "${sd_pfft_enable}" = "yes"; then

  # Prepare environment

  sd_esl_saved_CPPFLAGS="${CPPFLAGS}"
  sd_esl_saved_CFLAGS="${CFLAGS}"
  sd_esl_saved_CXXFLAGS="${CXXFLAGS}"
  sd_esl_saved_FCFLAGS="${FCFLAGS}"
  sd_esl_saved_LDFLAGS="${LDFLAGS}"
  sd_esl_saved_LIBS="${LIBS}"

  if test "${sd_pfft_init}" = "esl"; then
    { $as_echo "$as_me:${as_lineno-$LINENO}: will look for PFFT in the installed ESL Bundle" >&5
$as_echo "$as_me: will look for PFFT in the installed ESL Bundle" >&6;}

  CPPFLAGS="${CPPFLAGS} ${sd_esl_bundle_cppflags}"
  CFLAGS="${CFLAGS} ${sd_esl_bundle_cflags}"
  CXXFLAGS="${CXXFLAGS} ${sd_esl_bundle_cxxflags}"
  FCFLAGS="${FCFLAGS} ${sd_esl_bundle_fcflags}"
  LDFLAGS="${LDFLAGS} ${sd_esl_bundle_ldflags}"
  if test ! -z ""; then
    CPPFLAGS="${CPPFLAGS} -I${ESL_BUNDLE_PREFIX}/include/"
    FCFLAGS="${FCFLAGS} -I${ESL_BUNDLE_PREFIX}/include/"
  fi
  FCFLAGS="${FCFLAGS} -I/usr/include"


  LIBS="${sd_esl_bundle_libdirs} ${sd_pfft_libs_def} ${sd_esl_bundle_libs} ${LIBS}"

  else
    CPPFLAGS="${CPPFLAGS} ${sd_fftw3_cppflags} ${sd_pfft_cppflags}"
    CFLAGS="${CFLAGS} ${sd_fftw3_cflags} ${sd_pfft_cflags}"
    LDFLAGS="${LDFLAGS} ${sd_fftw3_ldflags} ${sd_pfft_ldflags}"
    LIBS="${sd_pfft_libs} ${sd_fftw3_libs} ${LIBS}"
  fi

  # Check PFFT MPI C API
  { $as_echo "$as_me:${as_lineno-$LINENO}: checking whether the PFFT library works" >&5
$as_echo_n "checking whether the PFFT library works... " >&6; }
  if test "${sd_mpi_enable}" = "yes" -a "${sd_fftw3_mpi_ok}" = "yes"; then
    ac_ext=c
ac_cpp='$CPP $CPPFLAGS'
ac_compile='$CC -c $CFLAGS $CPPFLAGS conftest.$ac_ext >&5'
ac_link='$CC -o conftest$ac_exeext $CFLAGS $CPPFLAGS $LDFLAGS conftest.$ac_ext $LIBS >&5'
ac_compiler_gnu=$ac_cv_c_compiler_gnu

    cat confdefs.h - <<_ACEOF >conftest.$ac_ext
/* end confdefs.h.  */

#       include <pfft.h>

#ifdef FC_DUMMY_MAIN
#ifndef FC_DUMMY_MAIN_EQ_F77
#  ifdef __cplusplus
     extern "C"
#  endif
   int FC_DUMMY_MAIN() { return 1; }
#endif
#endif
int
main ()
{

        pfft_init();

  ;
  return 0;
}
_ACEOF
if ac_fn_c_try_link "$LINENO"; then :
  sd_pfft_ok="yes"
else
  sd_pfft_ok="no"
fi
rm -f core conftest.err conftest.$ac_objext \
    conftest$ac_exeext conftest.$ac_ext
    ac_ext=c
ac_cpp='$CPP $CPPFLAGS'
ac_compile='$CC -c $CFLAGS $CPPFLAGS conftest.$ac_ext >&5'
ac_link='$CC -o conftest$ac_exeext $CFLAGS $CPPFLAGS $LDFLAGS conftest.$ac_ext $LIBS >&5'
ac_compiler_gnu=$ac_cv_c_compiler_gnu

  else
    sd_pfft_ok="no"
  fi
  { $as_echo "$as_me:${as_lineno-$LINENO}: result: ${sd_pfft_ok}" >&5
$as_echo "${sd_pfft_ok}" >&6; }

  # Restore environment

  CPPFLAGS="${sd_esl_saved_CPPFLAGS}"
  CFLAGS="${sd_esl_saved_CFLAGS}"
  CXXFLAGS="${sd_esl_saved_CXXFLAGS}"
  FCFLAGS="${sd_esl_saved_FCFLAGS}"
  LDFLAGS="${sd_esl_saved_LDFLAGS}"
  LIBS="${sd_esl_saved_LIBS}"



    if test "${sd_pfft_ok}" = "yes"; then
      if test "${sd_pfft_init}" = "esl"; then
        sd_esl_bundle_libs="${sd_pfft_libs_def} ${sd_esl_bundle_libs}"
      else
        LIBS="${sd_pfft_libs} ${LIBS}"
      fi
      LDFLAGS="${LDFLAGS} ${sd_pfft_ldflags}"


$as_echo "#define HAVE_PFFT 1" >>confdefs.h

    else
      if test "${sd_pfft_status}" = "optional" -a \
              "${sd_pfft_init}" = "def"; then
        sd_pfft_enable="no"
        sd_pfft_cppflags=""
        sd_pfft_cflags=""
        sd_pfft_ldflags=""
        sd_pfft_libs=""
      else
        { { $as_echo "$as_me:${as_lineno-$LINENO}: error: in \`$ac_pwd':" >&5
$as_echo "$as_me: error: in \`$ac_pwd':" >&2;}
as_fn_error $? "invalid PFFT configuration
See \`config.log' for more details" "$LINENO" 5; }
      fi
    fi
  else
    sd_pfft_enable="no"
    sd_pfft_cppflags=""
    sd_pfft_cflags=""
    sd_pfft_ldflags=""
    sd_pfft_libs=""
  fi

        if test "${sd_pfft_ok}" = "yes"; then
          sd_fft_cppflags="${sd_pfft_cppflags}"
          sd_fft_cflags="${sd_pfft_cflags}"
          sd_fft_fcflags="${sd_pfft_fcflags}"
          sd_fft_ldflags="${sd_pfft_ldflags}"
          sd_fft_libs="${sd_pfft_libs}"
          sd_fft_ok="yes"
        fi
        ;;
      *)
        as_fn_error $? "unsupported FFT flavor: '${sd_fft_flavor}'" "$LINENO" 5
        ;;
    esac

    test "${sd_fft_ok}" = "yes" && break

  done

  # Check that a working FFT implementation has been found
  if test "${sd_fft_ok}" = "yes"; then
    sd_fft_enable="yes"
    { $as_echo "$as_me:${as_lineno-$LINENO}: checking for the actual FFT flavor to use" >&5
$as_echo_n "checking for the actual FFT flavor to use... " >&6; }
    { $as_echo "$as_me:${as_lineno-$LINENO}: result: ${sd_fft_flavor}" >&5
$as_echo "${sd_fft_flavor}" >&6; }
  else
    as_fn_error $? "invalid FFT configuration
                  Please adjust configure options to point to a working FFT
                  installation or change the requested FFT flavor through the
                  --with-fft-flavor option." "$LINENO" 5
  fi

else
  echo "Skip FFT setting : linalg not enabled..."
fi

# ---------------------------------------------------------------------------- #
# ---------------------------------------------------------------------------- #

#
# Profiling and performance analysis
#

#ABI_MSG_SECTION([Profiling and performance analysis])

# ---------------------------------------------------------------------------- #

#
# Feature triggers (optional features)
#



  abi_sec_title="Feature triggers"

    abi_sec_spaces="                                                                      "
  abi_sec_tmp="${abi_sec_title}"
  while test "${abi_sec_tmp}" != ""; do
    abi_sec_spaces=`echo "${abi_sec_spaces}" | sed -e 's/^.//'`
    abi_sec_tmp=`echo "${abi_sec_tmp}" | sed -e 's/^.//'`
  done

  echo ""
  echo " =============================================================================="
  echo " === ${abi_sec_title}${abi_sec_spaces} ==="
  echo " =============================================================================="
  echo ""


# VERY IMPORTANT: THE ORDER IS CRITICAL - DO NOT CHANGE IT!

# Note: some of the following packages are mandatory to run Abinit with
#       full capabilities, although internal implementations may provide
#       limited functionality in some cases

                       # ------------------------------ #

# PAPI library support

  # Display configuration

  { $as_echo "$as_me:${as_lineno-$LINENO}: checking whether to enable PAPI" >&5
$as_echo_n "checking whether to enable PAPI... " >&6; }
  { $as_echo "$as_me:${as_lineno-$LINENO}: result: ${sd_papi_enable}" >&5
$as_echo "${sd_papi_enable}" >&6; }
  if test "${sd_papi_enable}" != "no"; then
    { $as_echo "$as_me:${as_lineno-$LINENO}: checking how PAPI parameters have been set" >&5
$as_echo_n "checking how PAPI parameters have been set... " >&6; }
    { $as_echo "$as_me:${as_lineno-$LINENO}: result: ${sd_papi_init}" >&5
$as_echo "${sd_papi_init}" >&6; }
    { $as_echo "$as_me:${as_lineno-$LINENO}: checking for PAPI C preprocessing flags" >&5
$as_echo_n "checking for PAPI C preprocessing flags... " >&6; }
    if test "${sd_papi_cppflags}" = ""; then
      { $as_echo "$as_me:${as_lineno-$LINENO}: result: none" >&5
$as_echo "none" >&6; }
    else
      { $as_echo "$as_me:${as_lineno-$LINENO}: result: ${sd_papi_cppflags}" >&5
$as_echo "${sd_papi_cppflags}" >&6; }
    fi
    { $as_echo "$as_me:${as_lineno-$LINENO}: checking for PAPI C flags" >&5
$as_echo_n "checking for PAPI C flags... " >&6; }
    if test "${sd_papi_cflags}" = ""; then
      { $as_echo "$as_me:${as_lineno-$LINENO}: result: none" >&5
$as_echo "none" >&6; }
    else
      { $as_echo "$as_me:${as_lineno-$LINENO}: result: ${sd_papi_cflags}" >&5
$as_echo "${sd_papi_cflags}" >&6; }
    fi
    { $as_echo "$as_me:${as_lineno-$LINENO}: checking for PAPI linker flags" >&5
$as_echo_n "checking for PAPI linker flags... " >&6; }
    if test "${sd_papi_ldflags}" = ""; then
      { $as_echo "$as_me:${as_lineno-$LINENO}: result: none" >&5
$as_echo "none" >&6; }
    else
      { $as_echo "$as_me:${as_lineno-$LINENO}: result: ${sd_papi_ldflags}" >&5
$as_echo "${sd_papi_ldflags}" >&6; }
    fi
    { $as_echo "$as_me:${as_lineno-$LINENO}: checking for PAPI library flags" >&5
$as_echo_n "checking for PAPI library flags... " >&6; }
    if test "${sd_papi_libs}" = ""; then
      { $as_echo "$as_me:${as_lineno-$LINENO}: result: none" >&5
$as_echo "none" >&6; }
    else
      { $as_echo "$as_me:${as_lineno-$LINENO}: result: ${sd_papi_libs}" >&5
$as_echo "${sd_papi_libs}" >&6; }
    fi
  fi


  # Check whether we can compile and link a simple program
  # and update build flags if successful
  if test "${sd_papi_enable}" = "auto" -o "${sd_papi_enable}" = "yes"; then

  # Prepare environment

  sd_esl_saved_CPPFLAGS="${CPPFLAGS}"
  sd_esl_saved_CFLAGS="${CFLAGS}"
  sd_esl_saved_CXXFLAGS="${CXXFLAGS}"
  sd_esl_saved_FCFLAGS="${FCFLAGS}"
  sd_esl_saved_LDFLAGS="${LDFLAGS}"
  sd_esl_saved_LIBS="${LIBS}"

  if test "${sd_papi_init}" = "esl"; then
    { $as_echo "$as_me:${as_lineno-$LINENO}: will look for PAPI in the installed ESL Bundle" >&5
$as_echo "$as_me: will look for PAPI in the installed ESL Bundle" >&6;}

  CPPFLAGS="${CPPFLAGS} ${sd_esl_bundle_cppflags}"
  CFLAGS="${CFLAGS} ${sd_esl_bundle_cflags}"
  CXXFLAGS="${CXXFLAGS} ${sd_esl_bundle_cxxflags}"
  FCFLAGS="${FCFLAGS} ${sd_esl_bundle_fcflags}"
  LDFLAGS="${LDFLAGS} ${sd_esl_bundle_ldflags}"
  if test ! -z ""; then
    CPPFLAGS="${CPPFLAGS} -I${ESL_BUNDLE_PREFIX}/include/"
    FCFLAGS="${FCFLAGS} -I${ESL_BUNDLE_PREFIX}/include/"
  fi
  FCFLAGS="${FCFLAGS} -I/usr/include"


  LIBS="${sd_esl_bundle_libdirs} ${sd_papi_libs_def} ${sd_esl_bundle_libs} ${LIBS}"

  else
    CPPFLAGS="${CPPFLAGS} ${sd_papi_cppflags}"
    CFLAGS="${CFLAGS} ${sd_papi_cflags}"
    LDFLAGS="${LDFLAGS} ${sd_papi_ldflags}"
    LIBS="${sd_papi_libs} ${LIBS}"
  fi

  # Add rt support if available on the machine
  if test "${PAPI_LIBS}" = ""; then
    ac_ext=c
ac_cpp='$CPP $CPPFLAGS'
ac_compile='$CC -c $CFLAGS $CPPFLAGS conftest.$ac_ext >&5'
ac_link='$CC -o conftest$ac_exeext $CFLAGS $CPPFLAGS $LDFLAGS conftest.$ac_ext $LIBS >&5'
ac_compiler_gnu=$ac_cv_c_compiler_gnu

    for ac_header in time.h
do :
  ac_fn_c_check_header_mongrel "$LINENO" "time.h" "ac_cv_header_time_h" "$ac_includes_default"
if test "x$ac_cv_header_time_h" = xyes; then :
  cat >>confdefs.h <<_ACEOF
#define HAVE_TIME_H 1
_ACEOF

fi

done

    { $as_echo "$as_me:${as_lineno-$LINENO}: checking for clock_gettime in -lrt" >&5
$as_echo_n "checking for clock_gettime in -lrt... " >&6; }
if ${ac_cv_lib_rt_clock_gettime+:} false; then :
  $as_echo_n "(cached) " >&6
else
  ac_check_lib_save_LIBS=$LIBS
LIBS="-lrt  $LIBS"
cat confdefs.h - <<_ACEOF >conftest.$ac_ext
/* end confdefs.h.  */

/* Override any GCC internal prototype to avoid an error.
   Use char because int might match the return type of a GCC
   builtin and then its argument prototype would still apply.  */
#ifdef __cplusplus
extern "C"
#endif
char clock_gettime ();
#ifdef FC_DUMMY_MAIN
#ifndef FC_DUMMY_MAIN_EQ_F77
#  ifdef __cplusplus
     extern "C"
#  endif
   int FC_DUMMY_MAIN() { return 1; }
#endif
#endif
int
main ()
{
return clock_gettime ();
  ;
  return 0;
}
_ACEOF
if ac_fn_c_try_link "$LINENO"; then :
  ac_cv_lib_rt_clock_gettime=yes
else
  ac_cv_lib_rt_clock_gettime=no
fi
rm -f core conftest.err conftest.$ac_objext \
    conftest$ac_exeext conftest.$ac_ext
LIBS=$ac_check_lib_save_LIBS
fi
{ $as_echo "$as_me:${as_lineno-$LINENO}: result: $ac_cv_lib_rt_clock_gettime" >&5
$as_echo "$ac_cv_lib_rt_clock_gettime" >&6; }
if test "x$ac_cv_lib_rt_clock_gettime" = xyes; then :
  sd_papi_rt_libs="-lrt"
else
  sd_papi_rt_libs=""
fi

    for ac_func in clock_gettime
do :
  ac_fn_c_check_func "$LINENO" "clock_gettime" "ac_cv_func_clock_gettime"
if test "x$ac_cv_func_clock_gettime" = xyes; then :
  cat >>confdefs.h <<_ACEOF
#define HAVE_CLOCK_GETTIME 1
_ACEOF

fi
done

    ac_ext=c
ac_cpp='$CPP $CPPFLAGS'
ac_compile='$CC -c $CFLAGS $CPPFLAGS conftest.$ac_ext >&5'
ac_link='$CC -o conftest$ac_exeext $CFLAGS $CPPFLAGS $LDFLAGS conftest.$ac_ext $LIBS >&5'
ac_compiler_gnu=$ac_cv_c_compiler_gnu

    sd_papi_libs="${sd_papi_rt_libs} ${sd_papi_libs}"
  fi

  # Check C headers
  ac_ext=c
ac_cpp='$CPP $CPPFLAGS'
ac_compile='$CC -c $CFLAGS $CPPFLAGS conftest.$ac_ext >&5'
ac_link='$CC -o conftest$ac_exeext $CFLAGS $CPPFLAGS $LDFLAGS conftest.$ac_ext $LIBS >&5'
ac_compiler_gnu=$ac_cv_c_compiler_gnu

  for ac_header in papi.h f90papi.h
do :
  as_ac_Header=`$as_echo "ac_cv_header_$ac_header" | $as_tr_sh`
ac_fn_c_check_header_mongrel "$LINENO" "$ac_header" "$as_ac_Header" "$ac_includes_default"
if eval test \"x\$"$as_ac_Header"\" = x"yes"; then :
  cat >>confdefs.h <<_ACEOF
#define `$as_echo "HAVE_$ac_header" | $as_tr_cpp` 1
_ACEOF
 sd_papi_has_hdrs="yes"
else
  sd_papi_has_hdrs="no"
fi

done

  ac_ext=c
ac_cpp='$CPP $CPPFLAGS'
ac_compile='$CC -c $CFLAGS $CPPFLAGS conftest.$ac_ext >&5'
ac_link='$CC -o conftest$ac_exeext $CFLAGS $CPPFLAGS $LDFLAGS conftest.$ac_ext $LIBS >&5'
ac_compiler_gnu=$ac_cv_c_compiler_gnu


  # Check Fortran support
  if test "${sd_papi_has_hdrs}" = "yes"; then
    ac_ext=${ac_fc_srcext-f}
ac_compile='$FC -c $FCFLAGS $ac_fcflags_srcext conftest.$ac_ext >&5'
ac_link='$FC -o conftest$ac_exeext $FCFLAGS $LDFLAGS $ac_fcflags_srcext conftest.$ac_ext $LIBS >&5'
ac_compiler_gnu=$ac_cv_fc_compiler_gnu

    { $as_echo "$as_me:${as_lineno-$LINENO}: checking whether the specified PAPI library works" >&5
$as_echo_n "checking whether the specified PAPI library works... " >&6; }
    cat > conftest.$ac_ext <<_ACEOF
      program main

#       if defined HAVE_F90PAPI_H
#       include "f90papi.h"
#       endif
        call PAPIf_library_init

      end
_ACEOF
if ac_fn_fc_try_link "$LINENO"; then :
  sd_papi_ok="yes"
else
  sd_papi_ok="no"
fi
rm -f core conftest.err conftest.$ac_objext \
    conftest$ac_exeext conftest.$ac_ext
    { $as_echo "$as_me:${as_lineno-$LINENO}: result: ${sd_papi_ok}" >&5
$as_echo "${sd_papi_ok}" >&6; }
    ac_ext=c
ac_cpp='$CPP $CPPFLAGS'
ac_compile='$CC -c $CFLAGS $CPPFLAGS conftest.$ac_ext >&5'
ac_link='$CC -o conftest$ac_exeext $CFLAGS $CPPFLAGS $LDFLAGS conftest.$ac_ext $LIBS >&5'
ac_compiler_gnu=$ac_cv_c_compiler_gnu

  fi

  # Restore environment

  CPPFLAGS="${sd_esl_saved_CPPFLAGS}"
  CFLAGS="${sd_esl_saved_CFLAGS}"
  CXXFLAGS="${sd_esl_saved_CXXFLAGS}"
  FCFLAGS="${sd_esl_saved_FCFLAGS}"
  LDFLAGS="${sd_esl_saved_LDFLAGS}"
  LIBS="${sd_esl_saved_LIBS}"



    if test "${sd_papi_ok}" = "yes"; then
      if test "${sd_papi_init}" = "esl"; then
        sd_esl_bundle_libs="${sd_papi_libs_def} ${sd_esl_bundle_libs}"
      else
        LIBS="${sd_papi_libs} ${LIBS}"
      fi
      LDFLAGS="${LDFLAGS} ${sd_papi_ldflags}"


$as_echo "#define HAVE_PAPI 1" >>confdefs.h

    else
      if test "${sd_papi_status}" = "optional" -a \
              "${sd_papi_init}" = "def"; then
        sd_papi_enable="no"
        sd_papi_cppflags=""
        sd_papi_cflags=""
        sd_papi_ldflags=""
        sd_papi_libs=""
      else
        { { $as_echo "$as_me:${as_lineno-$LINENO}: error: in \`$ac_pwd':" >&5
$as_echo "$as_me: error: in \`$ac_pwd':" >&2;}
as_fn_error $? "invalid PAPI configuration
See \`config.log' for more details" "$LINENO" 5; }
      fi
    fi
  else
    sd_papi_enable="no"
    sd_papi_cppflags=""
    sd_papi_cflags=""
    sd_papi_ldflags=""
    sd_papi_libs=""
  fi

{ $as_echo "$as_me:${as_lineno-$LINENO}: checking whether to enable the internal Abinit timer" >&5
$as_echo_n "checking whether to enable the internal Abinit timer... " >&6; }
{ $as_echo "$as_me:${as_lineno-$LINENO}: result: ${abi_timer_enable}" >&5
$as_echo "${abi_timer_enable}" >&6; }
if test "${sd_papi_ok}" != "yes"; then
  if test "${abi_timer_enable}" = "yes"; then

$as_echo "#define HAVE_TIMER_ABINIT 1" >>confdefs.h

  fi
fi

                       # ------------------------------ #

# HDF5 library support

  # Check whether we can compile and link a simple program
  # and update build flags if successful
  if test "${sd_hdf5_enable}" = "auto" -o "${sd_hdf5_enable}" = "yes"; then
    if test "${sd_hdf5_init}" != "env"; then

  if test "${H5CC}" = ""; then
    tmp_h5cc_search="h5cc"
    if test "${sd_mpi_ok}" = "yes"; then
      tmp_h5cc_search="h5pcc ${tmp_h5cc_search}"
    fi
    case "${sd_hdf5_init}" in
      dir)
        { $as_echo "$as_me:${as_lineno-$LINENO}: checking for a HDF5-aware compiler" >&5
$as_echo_n "checking for a HDF5-aware compiler... " >&6; }
        for tmp_h5cc in ${tmp_h5cc_search}; do
          if test -x "${sd_hdf5_prefix}/bin/${tmp_h5cc}"; then
            sd_hdf5_h5cc="${sd_hdf5_prefix}/bin/${tmp_h5cc}"
            break
          fi
        done
        if test "${sd_hdf5_h5cc}" = ""; then
          { $as_echo "$as_me:${as_lineno-$LINENO}: result: none" >&5
$as_echo "none" >&6; }
        else
          { $as_echo "$as_me:${as_lineno-$LINENO}: result: ${sd_hdf5_h5cc}" >&5
$as_echo "${sd_hdf5_h5cc}" >&6; }
        fi
        ;;
      *)
        for ac_prog in ${tmp_h5cc_search}
do
  # Extract the first word of "$ac_prog", so it can be a program name with args.
set dummy $ac_prog; ac_word=$2
{ $as_echo "$as_me:${as_lineno-$LINENO}: checking for $ac_word" >&5
$as_echo_n "checking for $ac_word... " >&6; }
if ${ac_cv_prog_sd_hdf5_h5cc+:} false; then :
  $as_echo_n "(cached) " >&6
else
  if test -n "$sd_hdf5_h5cc"; then
  ac_cv_prog_sd_hdf5_h5cc="$sd_hdf5_h5cc" # Let the user override the test.
else
as_save_IFS=$IFS; IFS=$PATH_SEPARATOR
for as_dir in $PATH
do
  IFS=$as_save_IFS
  test -z "$as_dir" && as_dir=.
    for ac_exec_ext in '' $ac_executable_extensions; do
  if as_fn_executable_p "$as_dir/$ac_word$ac_exec_ext"; then
    ac_cv_prog_sd_hdf5_h5cc="$ac_prog"
    $as_echo "$as_me:${as_lineno-$LINENO}: found $as_dir/$ac_word$ac_exec_ext" >&5
    break 2
  fi
done
  done
IFS=$as_save_IFS

fi
fi
sd_hdf5_h5cc=$ac_cv_prog_sd_hdf5_h5cc
if test -n "$sd_hdf5_h5cc"; then
  { $as_echo "$as_me:${as_lineno-$LINENO}: result: $sd_hdf5_h5cc" >&5
$as_echo "$sd_hdf5_h5cc" >&6; }
else
  { $as_echo "$as_me:${as_lineno-$LINENO}: result: no" >&5
$as_echo "no" >&6; }
fi


  test -n "$sd_hdf5_h5cc" && break
done

        ;;
    esac
    unset tmp_h5cc_search
    unset tmp_h5cc
  else
    ${H5CC} -show >/dev/null 2>&1
    if test "${?}" != "0"; then
      as_fn_error $? "invalid HDF5-aware compiler:
                  H5CC = '${H5CC}'" "$LINENO" 5
    fi
    sd_hdf5_h5cc="${H5CC}"
  fi   # H5CC = ""

  if test "${sd_hdf5_h5cc}" != ""; then

    { $as_echo "$as_me:${as_lineno-$LINENO}: checking for the HDF5 install prefix" >&5
$as_echo_n "checking for the HDF5 install prefix... " >&6; }
    if test "${sd_hdf5_prefix}" = ""; then
      sd_hdf5_prefix=`${sd_hdf5_h5cc} -showconfig | grep 'Installation point:' | awk '{print $NF}'`
    fi
    if test "${sd_hdf5_prefix}" = ""; then
      { $as_echo "$as_me:${as_lineno-$LINENO}: result: none" >&5
$as_echo "none" >&6; }
    else
      { $as_echo "$as_me:${as_lineno-$LINENO}: result: ${sd_hdf5_prefix}" >&5
$as_echo "${sd_hdf5_prefix}" >&6; }
    fi

    { $as_echo "$as_me:${as_lineno-$LINENO}: checking for the HDF5 build flavor" >&5
$as_echo_n "checking for the HDF5 build flavor... " >&6; }
    sd_hdf5_flavor=`${sd_hdf5_h5cc} -showconfig | grep 'Flavor name:' | awk '{print $NF}'`
    if test "${sd_hdf5_flavor}" = ""; then
      { $as_echo "$as_me:${as_lineno-$LINENO}: result: none" >&5
$as_echo "none" >&6; }
    else
      { $as_echo "$as_me:${as_lineno-$LINENO}: result: ${sd_hdf5_flavor}" >&5
$as_echo "${sd_hdf5_flavor}" >&6; }
    fi

    if test "${sd_hdf5_enable_fc}" = "yes"; then
      { $as_echo "$as_me:${as_lineno-$LINENO}: checking whether HDF5 supports Fortran" >&5
$as_echo_n "checking whether HDF5 supports Fortran... " >&6; }
      sd_hdf5_has_fc=`${sd_hdf5_h5cc} -showconfig | grep 'Fortran:' | awk '{print $NF}'`
      test "${sd_hdf5_has_fc}" = "" && sd_hdf5_has_fc="no"
      { $as_echo "$as_me:${as_lineno-$LINENO}: result: ${sd_hdf5_has_fc}" >&5
$as_echo "${sd_hdf5_has_fc}" >&6; }
    else
      sd_hdf5_has_fc="no"
    fi

    { $as_echo "$as_me:${as_lineno-$LINENO}: checking for HDF5 high-level libraries" >&5
$as_echo_n "checking for HDF5 high-level libraries... " >&6; }
    tmp_hdf5_hl=`${sd_hdf5_h5cc} -showconfig | grep 'High[ \-][Ll]evel library:' | awk '{print $NF}'`
    if test "${tmp_hdf5_hl}" = "yes"; then
      sd_hdf5_libs_hl="-lhdf5_hl"
      test "${sd_hdf5_enable_fc}" = "yes" -a "${sd_hdf5_has_fc}" = "yes" && \
        sd_hdf5_libs_hl="-lhdf5hl_fortran ${sd_hdf5_libs_hl}"
    fi
    if test "${sd_hdf5_libs_hl}" = ""; then
      { $as_echo "$as_me:${as_lineno-$LINENO}: result: none" >&5
$as_echo "none" >&6; }
    else
      { $as_echo "$as_me:${as_lineno-$LINENO}: result: ${sd_hdf5_libs_hl}" >&5
$as_echo "${sd_hdf5_libs_hl}" >&6; }
    fi
    unset tmp_hdf5_hl

    { $as_echo "$as_me:${as_lineno-$LINENO}: checking for HDF5 extra dependencies" >&5
$as_echo_n "checking for HDF5 extra dependencies... " >&6; }
    sd_hdf5_libs_extra=`${sd_hdf5_h5cc} -showconfig | grep 'Extra libraries: ' | sed -e 's/.*Extra libraries: //'`
    if test "${sd_hdf5_libs_extra}" = ""; then
      { $as_echo "$as_me:${as_lineno-$LINENO}: result: none" >&5
$as_echo "none" >&6; }
    else
      { $as_echo "$as_me:${as_lineno-$LINENO}: result: ${sd_hdf5_libs_extra}" >&5
$as_echo "${sd_hdf5_libs_extra}" >&6; }
    fi

    if test "${sd_mpi_ok}" = "yes"; then
      { $as_echo "$as_me:${as_lineno-$LINENO}: checking whether HDF5 supports MPI" >&5
$as_echo_n "checking whether HDF5 supports MPI... " >&6; }
      sd_hdf5_has_mpi=`${sd_hdf5_h5cc} -showconfig | grep 'Parallel HDF5:' | awk '{print $NF}'`
      test "${sd_hdf5_has_mpi}" = "" && sd_hdf5_has_mpi="no"
      { $as_echo "$as_me:${as_lineno-$LINENO}: result: ${sd_hdf5_has_mpi}" >&5
$as_echo "${sd_hdf5_has_mpi}" >&6; }
    else
      sd_hdf5_has_mpi="no"
    fi

    tmp_hdf5_incs=""
    tmp_hdf5_libdirs=""
    for arg in `${sd_hdf5_h5cc} -show`; do
      case ${arg} in
        -I*)
          tmp_hdf5_incs="${tmp_hdf5_incs} ${arg}"
          ;;
        -L*)
          tmp_hdf5_libdirs="${tmp_hdf5_libdirs} ${arg}"
        ;;
      esac
    done
    sd_hdf5_cppflags="${sd_hdf5_cppflags} ${tmp_hdf5_incs}"
    test "${sd_hdf5_enable_fc}" = "yes" && \
      sd_hdf5_fcflags="${sd_hdf5_fcflags} ${tmp_hdf5_incs}"
    sd_hdf5_libs="${tmp_hdf5_libdirs} ${sd_hdf5_libs_hl} ${sd_hdf5_libs} ${sd_hdf5_libs_extra}"
    unset arg
    unset tmp_hdf5_incs
    unset tmp_hdf5_libdirs

  fi   # sd_hdf5_cc != ""

    fi
    if test "${sd_hdf5_h5cc}" = ""; then
      if test "${sd_hdf5_cascade}" = "yes"; then

  sd_hdf5_cppflags="${sd_hdf5_cppflags_def}"
  sd_hdf5_cflags="${sd_hdf5_cflags_def}"
  sd_hdf5_cxxflags="${sd_hdf5_cxxflags_def}"
  test "${sd_hdf5_enable_fc}" = "yes" && \
    sd_hdf5_fcflags="${sd_hdf5_fcflags_def}"
  sd_hdf5_ldflags="${sd_hdf5_ldflags_def}"
  sd_hdf5_libs="${sd_hdf5_libs_def}"
  test ! -z "${HDF5_CPPFLAGS}" && sd_hdf5_cppflags="${HDF5_CPPFLAGS}"
  test ! -z "${HDF5_CFLAGS}" && sd_hdf5_cflags="${HDF5_CFLAGS}"
  test ! -z "${HDF5_CXXFLAGS}" && sd_hdf5_cxxflags="${HDF5_CXXFLAGS}"
  if test "${sd_hdf5_enable_fc}" = "yes"; then
    test ! -z "${HDF5_FCFLAGS}" && sd_hdf5_fcflags="${HDF5_FCFLAGS}"
  fi
  test ! -z "${HDF5_LDFLAGS}" && sd_hdf5_ldflags="${HDF5_LDFLAGS}"
  test ! -z "${HDF5_LIBS}" && sd_hdf5_libs="${HDF5_LIBS}"

      else
        { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: could not find the HDF5 C compiler
                    Please set the H5CC environment variable to a valid HDF5
                    C compiler (usually called 'h5cc' or 'h5pcc'), so that
                    essential information about your HDF5 installation will be
                    exposed to the build system of ABINIT." >&5
$as_echo "$as_me: WARNING: could not find the HDF5 C compiler
                    Please set the H5CC environment variable to a valid HDF5
                    C compiler (usually called 'h5cc' or 'h5pcc'), so that
                    essential information about your HDF5 installation will be
                    exposed to the build system of ABINIT." >&2;}
      fi
    fi


  { $as_echo "$as_me:${as_lineno-$LINENO}: checking whether to enable HDF5" >&5
$as_echo_n "checking whether to enable HDF5... " >&6; }
  { $as_echo "$as_me:${as_lineno-$LINENO}: result: ${sd_hdf5_enable}" >&5
$as_echo "${sd_hdf5_enable}" >&6; }
  if test "${sd_hdf5_enable}" != "no"; then
    { $as_echo "$as_me:${as_lineno-$LINENO}: checking how HDF5 parameters have been set" >&5
$as_echo_n "checking how HDF5 parameters have been set... " >&6; }
    { $as_echo "$as_me:${as_lineno-$LINENO}: result: ${sd_hdf5_init}" >&5
$as_echo "${sd_hdf5_init}" >&6; }
    { $as_echo "$as_me:${as_lineno-$LINENO}: checking for HDF5 C preprocessing flags" >&5
$as_echo_n "checking for HDF5 C preprocessing flags... " >&6; }
    if test "${sd_hdf5_cppflags}" = ""; then
      { $as_echo "$as_me:${as_lineno-$LINENO}: result: none" >&5
$as_echo "none" >&6; }
    else
      { $as_echo "$as_me:${as_lineno-$LINENO}: result: ${sd_hdf5_cppflags}" >&5
$as_echo "${sd_hdf5_cppflags}" >&6; }
    fi
    { $as_echo "$as_me:${as_lineno-$LINENO}: checking for HDF5 C flags" >&5
$as_echo_n "checking for HDF5 C flags... " >&6; }
    if test "${sd_hdf5_cflags}" = ""; then
      { $as_echo "$as_me:${as_lineno-$LINENO}: result: none" >&5
$as_echo "none" >&6; }
    else
      { $as_echo "$as_me:${as_lineno-$LINENO}: result: ${sd_hdf5_cflags}" >&5
$as_echo "${sd_hdf5_cflags}" >&6; }
    fi
    { $as_echo "$as_me:${as_lineno-$LINENO}: checking for HDF5 C++ flags" >&5
$as_echo_n "checking for HDF5 C++ flags... " >&6; }
    if test "${sd_hdf5_cxxflags}" = ""; then
      { $as_echo "$as_me:${as_lineno-$LINENO}: result: none" >&5
$as_echo "none" >&6; }
    else
      { $as_echo "$as_me:${as_lineno-$LINENO}: result: ${sd_hdf5_cxxflags}" >&5
$as_echo "${sd_hdf5_cxxflags}" >&6; }
    fi
    if test "${sd_hdf5_enable_fc}" = "yes"; then
      { $as_echo "$as_me:${as_lineno-$LINENO}: checking for HDF5 Fortran flags" >&5
$as_echo_n "checking for HDF5 Fortran flags... " >&6; }
      if test "${sd_hdf5_fcflags}" = ""; then
        { $as_echo "$as_me:${as_lineno-$LINENO}: result: none" >&5
$as_echo "none" >&6; }
      else
        { $as_echo "$as_me:${as_lineno-$LINENO}: result: ${sd_hdf5_fcflags}" >&5
$as_echo "${sd_hdf5_fcflags}" >&6; }
      fi
    fi
    { $as_echo "$as_me:${as_lineno-$LINENO}: checking for HDF5 linker flags" >&5
$as_echo_n "checking for HDF5 linker flags... " >&6; }
    if test "${sd_hdf5_ldflags}" = ""; then
      { $as_echo "$as_me:${as_lineno-$LINENO}: result: none" >&5
$as_echo "none" >&6; }
    else
      { $as_echo "$as_me:${as_lineno-$LINENO}: result: ${sd_hdf5_ldflags}" >&5
$as_echo "${sd_hdf5_ldflags}" >&6; }
    fi
    { $as_echo "$as_me:${as_lineno-$LINENO}: checking for HDF5 library flags" >&5
$as_echo_n "checking for HDF5 library flags... " >&6; }
    if test "${sd_hdf5_libs}" = ""; then
      { $as_echo "$as_me:${as_lineno-$LINENO}: result: none" >&5
$as_echo "none" >&6; }
    else
      { $as_echo "$as_me:${as_lineno-$LINENO}: result: ${sd_hdf5_libs}" >&5
$as_echo "${sd_hdf5_libs}" >&6; }
    fi
  fi


  # Prepare environment

  sd_esl_saved_CPPFLAGS="${CPPFLAGS}"
  sd_esl_saved_CFLAGS="${CFLAGS}"
  sd_esl_saved_CXXFLAGS="${CXXFLAGS}"
  sd_esl_saved_FCFLAGS="${FCFLAGS}"
  sd_esl_saved_LDFLAGS="${LDFLAGS}"
  sd_esl_saved_LIBS="${LIBS}"

  CPPFLAGS="${CPPFLAGS} ${sd_hdf5_cppflags}"
  CFLAGS="${CFLAGS} ${sd_hdf5_cflags}"
  CXXFLAGS="${CFLAGS} ${sd_hdf5_cxxflags}"
  FCFLAGS="${FCFLAGS} ${sd_hdf5_fcflags}"
  LDFLAGS="${LDFLAGS} ${sd_hdf5_ldflags}"
  LIBS="${sd_hdf5_libs} ${LIBS}"

  # Look for hdf5.h
  for ac_header in hdf5.h
do :
  ac_fn_c_check_header_mongrel "$LINENO" "hdf5.h" "ac_cv_header_hdf5_h" "$ac_includes_default"
if test "x$ac_cv_header_hdf5_h" = xyes; then :
  cat >>confdefs.h <<_ACEOF
#define HAVE_HDF5_H 1
_ACEOF
 sd_hdf5_hdr_ok="yes"
else
  sd_hdf5_hdr_ok="no"
fi

done

  if test "${sd_hdf5_hdr_ok}" != "yes"; then
    { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: could not find the HDF5 C header
                  Detecting HDF5 is a complex task due to the very high number
                  of possible configurations. If you do want to use HDF5,
                  please help the build system by pointing correctly the
                  HDF5_CPPFLAGS, HDF5_FCFLAGS and HDF5_LIBS parameters to
                  where the desired hdf5.h and libhdf5.* files are installed." >&5
$as_echo "$as_me: WARNING: could not find the HDF5 C header
                  Detecting HDF5 is a complex task due to the very high number
                  of possible configurations. If you do want to use HDF5,
                  please help the build system by pointing correctly the
                  HDF5_CPPFLAGS, HDF5_FCFLAGS and HDF5_LIBS parameters to
                  where the desired hdf5.h and libhdf5.* files are installed." >&2;}
  fi

  # Check HDF5 C API
  { $as_echo "$as_me:${as_lineno-$LINENO}: checking whether the HDF5 C interface works" >&5
$as_echo_n "checking whether the HDF5 C interface works... " >&6; }
  ac_ext=c
ac_cpp='$CPP $CPPFLAGS'
ac_compile='$CC -c $CFLAGS $CPPFLAGS conftest.$ac_ext >&5'
ac_link='$CC -o conftest$ac_exeext $CFLAGS $CPPFLAGS $LDFLAGS conftest.$ac_ext $LIBS >&5'
ac_compiler_gnu=$ac_cv_c_compiler_gnu

  cat confdefs.h - <<_ACEOF >conftest.$ac_ext
/* end confdefs.h.  */

#include <hdf5.h>

#ifdef FC_DUMMY_MAIN
#ifndef FC_DUMMY_MAIN_EQ_F77
#  ifdef __cplusplus
     extern "C"
#  endif
   int FC_DUMMY_MAIN() { return 1; }
#endif
#endif
int
main ()
{

      hid_t file_id;
      file_id = H5Fcreate("conftest.h5", H5F_ACC_TRUNC, H5P_DEFAULT, H5P_DEFAULT);

  ;
  return 0;
}
_ACEOF
if ac_fn_c_try_link "$LINENO"; then :
  sd_hdf5_c_ok="yes"
else
  sd_hdf5_c_ok="no"
fi
rm -f core conftest.err conftest.$ac_objext \
    conftest$ac_exeext conftest.$ac_ext
  ac_ext=c
ac_cpp='$CPP $CPPFLAGS'
ac_compile='$CC -c $CFLAGS $CPPFLAGS conftest.$ac_ext >&5'
ac_link='$CC -o conftest$ac_exeext $CFLAGS $CPPFLAGS $LDFLAGS conftest.$ac_ext $LIBS >&5'
ac_compiler_gnu=$ac_cv_c_compiler_gnu

  { $as_echo "$as_me:${as_lineno-$LINENO}: result: ${sd_hdf5_c_ok}" >&5
$as_echo "${sd_hdf5_c_ok}" >&6; }

  # Check HDF5 Fortran API
  if test "${sd_hdf5_c_ok}" = "yes" -a "${sd_hdf5_enable_fc}" = "yes"; then
    { $as_echo "$as_me:${as_lineno-$LINENO}: checking whether the HDF5 Fortran interface works" >&5
$as_echo_n "checking whether the HDF5 Fortran interface works... " >&6; }
    for tmp_incs in "" "-I/usr/include"; do
      FCFLAGS="${FCFLAGS} ${tmp_incs}"
      ac_ext=${ac_fc_srcext-f}
ac_compile='$FC -c $FCFLAGS $ac_fcflags_srcext conftest.$ac_ext >&5'
ac_link='$FC -o conftest$ac_exeext $FCFLAGS $LDFLAGS $ac_fcflags_srcext conftest.$ac_ext $LIBS >&5'
ac_compiler_gnu=$ac_cv_fc_compiler_gnu

      cat > conftest.$ac_ext <<_ACEOF
      program main

          use hdf5
          integer(hid_t) :: file_id
          integer :: ierr
          call h5open_f(ierr)
          call h5fcreate_f("conftest.h5f", H5F_ACC_TRUNC_F, file_id, ierr)

      end
_ACEOF
if ac_fn_fc_try_link "$LINENO"; then :
  sd_hdf5_fortran_ok="yes"
else
  sd_hdf5_fortran_ok="no"
fi
rm -f core conftest.err conftest.$ac_objext \
    conftest$ac_exeext conftest.$ac_ext
      ac_ext=c
ac_cpp='$CPP $CPPFLAGS'
ac_compile='$CC -c $CFLAGS $CPPFLAGS conftest.$ac_ext >&5'
ac_link='$CC -o conftest$ac_exeext $CFLAGS $CPPFLAGS $LDFLAGS conftest.$ac_ext $LIBS >&5'
ac_compiler_gnu=$ac_cv_c_compiler_gnu

      if test "${sd_hdf5_fortran_ok}" = "yes"; then
        test "${sd_fc_sys_incpath}" = "" && sd_fc_sys_incpath="${tmp_incs}"
        break
      fi
    done
    { $as_echo "$as_me:${as_lineno-$LINENO}: result: ${sd_hdf5_fortran_ok}" >&5
$as_echo "${sd_hdf5_fortran_ok}" >&6; }
  fi
  unset tmp_incs

  # Check HDF5 parallel API
  if test "${sd_hdf5_c_ok}" = "yes" -a "${sd_mpi_ok}" = "yes" -a \
          "${sd_hdf5_has_mpi}" = "yes"; then
    { $as_echo "$as_me:${as_lineno-$LINENO}: checking whether the parallel HDF5 API works" >&5
$as_echo_n "checking whether the parallel HDF5 API works... " >&6; }
    ac_ext=c
ac_cpp='$CPP $CPPFLAGS'
ac_compile='$CC -c $CFLAGS $CPPFLAGS conftest.$ac_ext >&5'
ac_link='$CC -o conftest$ac_exeext $CFLAGS $CPPFLAGS $LDFLAGS conftest.$ac_ext $LIBS >&5'
ac_compiler_gnu=$ac_cv_c_compiler_gnu

    cat confdefs.h - <<_ACEOF >conftest.$ac_ext
/* end confdefs.h.  */

#include <mpi.h>
#include <hdf5.h>

#ifdef FC_DUMMY_MAIN
#ifndef FC_DUMMY_MAIN_EQ_F77
#  ifdef __cplusplus
     extern "C"
#  endif
   int FC_DUMMY_MAIN() { return 1; }
#endif
#endif
int
main ()
{

        hid_t acc_tpl, file_id;
        herr_t ret;
        MPI_Comm comm = MPI_COMM_WORLD;
        MPI_Info info = MPI_INFO_NULL;
        acc_tpl = H5Pcreate(H5P_FILE_ACCESS);
        ret = H5Pset_fapl_mpio(acc_tpl, comm, info);
        file_id = H5Fcreate("conftest.h5", H5F_ACC_TRUNC, H5P_DEFAULT, acc_tpl);

  ;
  return 0;
}
_ACEOF
if ac_fn_c_try_link "$LINENO"; then :
  sd_hdf5_mpi_ok="yes"
else
  sd_hdf5_mpi_ok="no"
fi
rm -f core conftest.err conftest.$ac_objext \
    conftest$ac_exeext conftest.$ac_ext
    ac_ext=c
ac_cpp='$CPP $CPPFLAGS'
ac_compile='$CC -c $CFLAGS $CPPFLAGS conftest.$ac_ext >&5'
ac_link='$CC -o conftest$ac_exeext $CFLAGS $CPPFLAGS $LDFLAGS conftest.$ac_ext $LIBS >&5'
ac_compiler_gnu=$ac_cv_c_compiler_gnu

    { $as_echo "$as_me:${as_lineno-$LINENO}: result: ${sd_hdf5_mpi_ok}" >&5
$as_echo "${sd_hdf5_mpi_ok}" >&6; }
  else
    sd_hdf5_has_mpi="no"
    sd_hdf5_mpi_ok="no"
  fi

  # Combine the available results
  sd_hdf5_ok="no"
  if test "${sd_hdf5_enable_fc}" = "yes"; then
    if test "${sd_hdf5_c_ok}" = "yes" -a \
            "${sd_hdf5_fortran_ok}" = "yes"; then
      sd_hdf5_ok="yes"
    fi
  else
    if test "${sd_hdf5_c_ok}" = "yes"; then
      sd_hdf5_ok="yes"
    fi
  fi

  # Restore environment

  CPPFLAGS="${sd_esl_saved_CPPFLAGS}"
  CFLAGS="${sd_esl_saved_CFLAGS}"
  CXXFLAGS="${sd_esl_saved_CXXFLAGS}"
  FCFLAGS="${sd_esl_saved_FCFLAGS}"
  LDFLAGS="${sd_esl_saved_LDFLAGS}"
  LIBS="${sd_esl_saved_LIBS}"



    if test "${sd_hdf5_ok}" = "yes"; then
      CPPFLAGS="${CPPFLAGS} ${sd_hdf5_cppflags}"
      CFLAGS="${CFLAGS} ${sd_hdf5_cflags}"
      CXXFLAGS="${CXXFLAGS} ${sd_hdf5_cxxflags}"
      FCFLAGS="${FCFLAGS} ${sd_hdf5_fcflags}"
      LDFLAGS="${LDFLAGS} ${sd_hdf5_ldflags}"
      LIBS="${sd_hdf5_libs} ${LIBS}"


$as_echo "#define HAVE_HDF5 1" >>confdefs.h

      if test "${sd_hdf5_mpi_ok}" = "yes"; then

$as_echo "#define HAVE_HDF5_MPI 1" >>confdefs.h

      fi
    else
      if test "${sd_hdf5_status}" = "optional" -a \
              "${sd_hdf5_init}" = "def"; then
        sd_hdf5_enable="no"
        sd_hdf5_cppflags=""
        sd_hdf5_cflags=""
        sd_hdf5_cxxflags=""
        sd_hdf5_fcflags=""
        sd_hdf5_ldflags=""
        sd_hdf5_libs=""
      else
        if test "${sd_hdf5_policy}" = "fail"; then
              { { $as_echo "$as_me:${as_lineno-$LINENO}: error: in \`$ac_pwd':" >&5
$as_echo "$as_me: error: in \`$ac_pwd':" >&2;}
as_fn_error $? "invalid HDF5 configuration
See \`config.log' for more details" "$LINENO" 5; }
        else
              { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: invalid HDF5 configuration" >&5
$as_echo "$as_me: WARNING: invalid HDF5 configuration" >&2;}
        fi
      fi
    fi
  else
    sd_hdf5_enable="no"
    sd_hdf5_cppflags=""
    sd_hdf5_cflags=""
    sd_hdf5_cxxflags=""
    sd_hdf5_fcflags=""
    sd_hdf5_ldflags=""
    sd_hdf5_libs=""
  fi


{ $as_echo "$as_me:${as_lineno-$LINENO}: " >&5
$as_echo "$as_me: " >&6;}
{ $as_echo "$as_me:${as_lineno-$LINENO}: dumping HDF5 parameters for diagnostics" >&5
$as_echo "$as_me: dumping HDF5 parameters for diagnostics" >&6;}
{ $as_echo "$as_me:${as_lineno-$LINENO}: ---------------------------------------" >&5
$as_echo "$as_me: ---------------------------------------" >&6;}
