main ()
{

      int *argc = NULL;
      char ***argv = NULL;
      MPI_Init(argc, argv);

  ;
  return 0;
}
_ACEOF
if ac_fn_c_try_link "$LINENO"; then :
  sd_mpi_cc_api_ok="yes"
else
  sd_mpi_cc_api_ok="no"
