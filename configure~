  # Triggers for enable_exports
  { $as_echo "$as_me:${as_lineno-$LINENO}: checking whether to activate build of exported libraries" >&5
$as_echo_n "checking whether to activate build of exported libraries... " >&6; }
  { $as_echo "$as_me:${as_lineno-$LINENO}: result: ${abi_exports_enable}" >&5
$as_echo "${abi_exports_enable}" >&6; }

