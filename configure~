    XPPFLAGS_DEBUG="${abi_env_XPPFLAGS_DEBUG}"

    XPPFLAGS_EXTRA="${abi_env_XPPFLAGS_EXTRA}"

    XPPFLAGS_OPTIM="${abi_env_XPPFLAGS_OPTIM}"



  # Forward information to the compiler
  if test "${abi_mpi_iallreduce_ok}" = "yes"; then

$as_echo "#define HAVE_MPI_IALLREDUCE 1" >>confdefs.h

  else
    { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Your MPI library does not support non-blocking communications. The wall time of certain algorithms will increase with the number of MPI processes. It is strongly suggested to use a more recent MPI2+ library!" >&5
$as_echo "$as_me: WARNING: Your MPI library does not support non-blocking communications. The wall time of certain algorithms will increase with the number of MPI processes. It is strongly suggested to use a more recent MPI2+ library!" >&2;}
  fi


    fi # sd_mpi_ok

  fi # sd_mpi_enable

if test "${abi_mpi_enable}" = "yes"; then

  { $as_echo "$as_me:${as_lineno-$LINENO}: " >&5
$as_echo "$as_me: " >&6;}
