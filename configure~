  # mkl_omatadd family
  { $as_echo "$as_me:${as_lineno-$LINENO}: checking for mkl_omatadd in the specified libraries" >&5
$as_echo_n "checking for mkl_omatadd in the specified libraries... " >&6; }
  ac_ext=${ac_fc_srcext-f}
