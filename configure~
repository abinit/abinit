    abi_env_FCFLAGS_DEBUG="${FCFLAGS_DEBUG}"

    abi_env_FCFLAGS_EXTRA="${FCFLAGS_EXTRA}"

    abi_env_FCFLAGS_FIXEDFORM="${FCFLAGS_FIXEDFORM}"

    abi_env_FCFLAGS_FREEFORM="${FCFLAGS_FREEFORM}"

