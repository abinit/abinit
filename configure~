  { $as_echo "$as_me:${as_lineno-$LINENO}: result: ${sd_linalg_mkl_has_omatcopy}" >&5
$as_echo "${sd_linalg_mkl_has_omatcopy}" >&6; }

  if test "${sd_linalg_mkl_has_omatcopy}" = "yes"; then

