    FCFLAGS="${abi_env_FCFLAGS}"

    FCFLAGS_DEBUG="${abi_env_FCFLAGS_DEBUG}"

    FCFLAGS_EXTRA="${abi_env_FCFLAGS_EXTRA}"

    FCFLAGS_FIXEDFORM="${abi_env_FCFLAGS_FIXEDFORM}"

