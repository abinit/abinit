$as_echo "$as_me:    * sd_hdf5_has_mpi = ${sd_hdf5_has_mpi}" >&6;}
{ $as_echo "$as_me:${as_lineno-$LINENO}: " >&5
$as_echo "$as_me: " >&6;}

