$as_echo "$as_me:    * sd_hdf5_enable = ${sd_hdf5_enable}" >&6;}
{ $as_echo "$as_me:${as_lineno-$LINENO}:    * sd_hdf5_status = ${sd_hdf5_status}" >&5
$as_echo "$as_me:    * sd_hdf5_status = ${sd_hdf5_status}" >&6;}
