  { $as_echo "$as_me:${as_lineno-$LINENO}: result: ${sd_linalg_has_axpby}" >&5
$as_echo "${sd_linalg_has_axpby}" >&6; }

  if test "${sd_linalg_has_axpby}" = "yes"; then

$as_echo "#define HAVE_LINALG_AXPBY 1" >>confdefs.h

  fi

  # gemm3m family
  { $as_echo "$as_me:${as_lineno-$LINENO}: checking for GEMM3M in the BLAS libraries" >&5
