          use ISO_FORTRAN_ENV, only : int16,int32,int64

      end
_ACEOF
if ac_fn_fc_try_link "$LINENO"; then :
  fc_has_iso_fortran_2008="yes"
fi
rm -f core conftest.err conftest.$ac_objext \
