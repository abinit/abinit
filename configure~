    FPPFLAGS_EXTRA="${abi_env_FPPFLAGS_EXTRA}"

    FPPFLAGS_HINTS="${abi_env_FPPFLAGS_HINTS}"

    FPPFLAGS_OPTIM="${abi_env_FPPFLAGS_OPTIM}"

    LD="${abi_env_LD}"

