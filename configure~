$as_echo "$as_me:   * with_mpi           = '${with_mpi}'" >&6;}
  { $as_echo "$as_me:${as_lineno-$LINENO}:   * with_mpi_level     = '${with_mpi_level}'" >&5
$as_echo "$as_me:   * with_mpi_level     = '${with_mpi_level}'" >&6;}
  { $as_echo "$as_me:${as_lineno-$LINENO}: " >&5
