  if test "${abi_lotf_enable}" = "yes"; then
    { $as_echo "$as_me:${as_lineno-$LINENO}: triggering the 'DO_BUILD_68_LOTF' conditional" >&5
$as_echo "$as_me: triggering the 'DO_BUILD_68_LOTF' conditional" >&6;}
