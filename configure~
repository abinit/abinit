$as_echo "$as_me:   * deprecated option --enable-exports" >&6;}
  if test "${enable_exports}" != ""; then
    { $as_echo "$as_me:${as_lineno-$LINENO}:     >>> --enable-exports = '${enable_exports}' will soon be forbidden!" >&5
$as_echo "$as_me:     >>> --enable-exports = '${enable_exports}' will soon be forbidden!" >&6;}
    abi_opt_deprecated_count=`expr ${abi_opt_deprecated_count} + 1`
    abi_opt_deprecated_used="${abi_opt_deprecated_used} enable_exports"
  fi

  # --enable-triqs-v1-4 deprecated
  { $as_echo "$as_me:${as_lineno-$LINENO}:   * deprecated option --enable-triqs-v1-4" >&5
$as_echo "$as_me:   * deprecated option --enable-triqs-v1-4" >&6;}
  if test "${enable_triqs_v1_4}" != ""; then
    { $as_echo "$as_me:${as_lineno-$LINENO}:     >>> --enable-triqs-v1-4 = '${enable_triqs_v1_4}' will soon be forbidden!" >&5
$as_echo "$as_me:     >>> --enable-triqs-v1-4 = '${enable_triqs_v1_4}' will soon be forbidden!" >&6;}
    abi_opt_deprecated_count=`expr ${abi_opt_deprecated_count} + 1`
    abi_opt_deprecated_used="${abi_opt_deprecated_used} enable_triqs_v1_4"
  fi

  # --enable-triqs-v2-0 deprecated
  { $as_echo "$as_me:${as_lineno-$LINENO}:   * deprecated option --enable-triqs-v2-0" >&5
$as_echo "$as_me:   * deprecated option --enable-triqs-v2-0" >&6;}
  if test "${enable_triqs_v2_0}" != ""; then
    { $as_echo "$as_me:${as_lineno-$LINENO}:     >>> --enable-triqs-v2-0 = '${enable_triqs_v2_0}' will soon be forbidden!" >&5
$as_echo "$as_me:     >>> --enable-triqs-v2-0 = '${enable_triqs_v2_0}' will soon be forbidden!" >&6;}
    abi_opt_deprecated_count=`expr ${abi_opt_deprecated_count} + 1`
    abi_opt_deprecated_used="${abi_opt_deprecated_used} enable_triqs_v2_0"
  fi



