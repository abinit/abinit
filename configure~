  fi

      if test "${sd_linalg_has_elpa_2017_f2008}" != "yes"; then
        sd_linalg_has_elpa="no"
      fi
    # ===== Old API
    else

  sd_linalg_has_elpa_2017_legacy="unknown"

  { $as_echo "$as_me:${as_lineno-$LINENO}: checking for ELPA 2017+ legacy API support in the specified libraries" >&5
$as_echo_n "checking for ELPA 2017+ legacy API support in the specified libraries... " >&6; }
