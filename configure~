  fi

  fi


  fi

  # Validate the GPU linear algebra support
  if test "${sd_linalg_has_magma}" = "yes"; then
    sd_linalg_gpu_ok="yes"
  else
    sd_linalg_gpu_ok="no"
  fi

  # Compose a flavor from the identified components
  if test "${sd_linalg_flavor_init}" = "def"; then
    if test "${sd_linalg_serial_ok}" = "yes"; then
      if test "${sd_linalg_mkl_has_imatcopy}" = "yes" -o \
              "${sd_linalg_mkl_has_omatcopy}" = "yes" -o \
              "${sd_linalg_mkl_has_omatadd}" = "yes"; then
        sd_linalg_flavor="mkl"
      else
        sd_linalg_flavor="netlib"
      fi
      if test "${sd_linalg_mpi_ok}" = "yes"; then
        if test "${sd_linalg_has_plasma}" = "yes"; then
          sd_linalg_flavor="${sd_linalg_flavor}+plasma"
        fi
      fi
      if test "${sd_linalg_mpiacc_ok}" = "yes"; then
        if test "${sd_linalg_has_elpa}" = "yes"; then
          sd_linalg_flavor="${sd_linalg_flavor}+elpa"
        fi
      fi
      if test "${sd_linalg_gpu_ok}" = "yes"; then
        if test "${sd_linalg_has_magma}" = "yes"; then
          sd_linalg_flavor="${sd_linalg_flavor}+magma"
        fi
      fi
    else
      sd_linalg_flavor="none"
    fi
  fi

  # Restore environment

  CPPFLAGS="${sd_esl_saved_CPPFLAGS}"
  CFLAGS="${sd_esl_saved_CFLAGS}"
  CXXFLAGS="${sd_esl_saved_CXXFLAGS}"
  FCFLAGS="${sd_esl_saved_FCFLAGS}"
  LDFLAGS="${sd_esl_saved_LDFLAGS}"
  LIBS="${sd_esl_saved_LIBS}"


        ;;
      *)
        as_fn_error $? "unsupported linear algebra init type: '${sd_linalg_init}'" "$LINENO" 5
        ;;
    esac

  if test "${sd_linalg_enable}" != "no"; then
    { $as_echo "$as_me:${as_lineno-$LINENO}: checking how linear algebra parameters have been set" >&5
$as_echo_n "checking how linear algebra parameters have been set... " >&6; }
    { $as_echo "$as_me:${as_lineno-$LINENO}: result: ${sd_linalg_init} (flavor: ${sd_linalg_flavor_init})" >&5
$as_echo "${sd_linalg_init} (flavor: ${sd_linalg_flavor_init})" >&6; }
    { $as_echo "$as_me:${as_lineno-$LINENO}: checking for the actual linear algebra flavor" >&5
$as_echo_n "checking for the actual linear algebra flavor... " >&6; }
    if test "${sd_linalg_flavor}" = ""; then
      { $as_echo "$as_me:${as_lineno-$LINENO}: result: none" >&5
$as_echo "none" >&6; }
    else
      { $as_echo "$as_me:${as_lineno-$LINENO}: result: ${sd_linalg_flavor}" >&5
$as_echo "${sd_linalg_flavor}" >&6; }
    fi
    { $as_echo "$as_me:${as_lineno-$LINENO}: checking for linear algebra C preprocessing flags" >&5
$as_echo_n "checking for linear algebra C preprocessing flags... " >&6; }
    if test "${sd_linalg_cppflags}" = ""; then
      { $as_echo "$as_me:${as_lineno-$LINENO}: result: none" >&5
$as_echo "none" >&6; }
    else
      { $as_echo "$as_me:${as_lineno-$LINENO}: result: ${sd_linalg_cppflags}" >&5
$as_echo "${sd_linalg_cppflags}" >&6; }
    fi
    { $as_echo "$as_me:${as_lineno-$LINENO}: checking for linear algebra C flags" >&5
$as_echo_n "checking for linear algebra C flags... " >&6; }
    if test "${sd_linalg_cflags}" = ""; then
      { $as_echo "$as_me:${as_lineno-$LINENO}: result: none" >&5
$as_echo "none" >&6; }
    else
      { $as_echo "$as_me:${as_lineno-$LINENO}: result: ${sd_linalg_cflags}" >&5
$as_echo "${sd_linalg_cflags}" >&6; }
    fi
    { $as_echo "$as_me:${as_lineno-$LINENO}: checking for linear algebra C++ flags" >&5
$as_echo_n "checking for linear algebra C++ flags... " >&6; }
    if test "${sd_linalg_cxxflags}" = ""; then
      { $as_echo "$as_me:${as_lineno-$LINENO}: result: none" >&5
$as_echo "none" >&6; }
    else
      { $as_echo "$as_me:${as_lineno-$LINENO}: result: ${sd_linalg_cxxflags}" >&5
$as_echo "${sd_linalg_cxxflags}" >&6; }
    fi
    { $as_echo "$as_me:${as_lineno-$LINENO}: checking for linear algebra Fortran flags" >&5
$as_echo_n "checking for linear algebra Fortran flags... " >&6; }
    if test "${sd_linalg_fcflags}" = ""; then
      { $as_echo "$as_me:${as_lineno-$LINENO}: result: none" >&5
$as_echo "none" >&6; }
    else
      { $as_echo "$as_me:${as_lineno-$LINENO}: result: ${sd_linalg_fcflags}" >&5
$as_echo "${sd_linalg_fcflags}" >&6; }
    fi
    { $as_echo "$as_me:${as_lineno-$LINENO}: checking for linear algebra linker flags" >&5
$as_echo_n "checking for linear algebra linker flags... " >&6; }
    if test "${sd_linalg_ldflags}" = ""; then
      { $as_echo "$as_me:${as_lineno-$LINENO}: result: none" >&5
$as_echo "none" >&6; }
    else
      { $as_echo "$as_me:${as_lineno-$LINENO}: result: ${sd_linalg_ldflags}" >&5
$as_echo "${sd_linalg_ldflags}" >&6; }
    fi
    { $as_echo "$as_me:${as_lineno-$LINENO}: checking for linear algebra library flags" >&5
$as_echo_n "checking for linear algebra library flags... " >&6; }
    if test "${sd_linalg_libs}" = ""; then
      { $as_echo "$as_me:${as_lineno-$LINENO}: result: none" >&5
$as_echo "none" >&6; }
    else
      { $as_echo "$as_me:${as_lineno-$LINENO}: result: ${sd_linalg_libs}" >&5
$as_echo "${sd_linalg_libs}" >&6; }
    fi
  fi


    # Check if minimum requirements are fulfilled
    if test "${sd_linalg_serial_ok}" != "yes"; then
      case "${sd_linalg_policy}" in
        fail)
          if test "${sd_linalg_enable}" = "auto"; then
            sd_linalg_enable="no"
          else
            as_fn_error $? "linear algebra does not work" "$LINENO" 5
          fi
          ;;
        skip)
          sd_linalg_enable="no"
          ;;
        warn)
          { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: linear algebra does not work" >&5
$as_echo "$as_me: WARNING: linear algebra does not work" >&2;}
          ;;
      esac
    fi

  fi   # sd_linalg_enable = auto | yes

  # Wipe-out configuration if linear algebra is finally disabled
  if test "${sd_linalg_enable}" = "no"; then
    sd_linalg_flavor="none"
    sd_linalg_cppflags=""
    sd_linalg_cflags=""
    sd_linalg_fcflags=""
    sd_linalg_ldflags=""
    sd_linalg_libs=""
  fi

  # Check that parallel minimum requirements are fulfilled, when relevant
  if test "${sd_linalg_serial_ok}" = "yes" -a "${sd_mpi_ok}" = "yes"; then
    if test "${sd_linalg_mpi_ok}" != "yes"; then
      { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: parallel linear algebra is not available" >&5
$as_echo "$as_me: WARNING: parallel linear algebra is not available" >&2;}
    fi
  fi

  # Define vendor-specific C preprocessing options
  tmp_linalg_iter=`echo "${sd_linalg_flavor}" | sed -e 's/+/ /g'`
  for tmp_linalg_vendor in ${tmp_linalg_iter}; do
    case "${tmp_linalg_vendor}" in
      asl)

$as_echo "#define HAVE_LINALG_ASL 1" >>confdefs.h

        ;;
      essl)

$as_echo "#define HAVE_LINALG_ESSL 1" >>confdefs.h

        ;;
    esac
  done
  unset tmp_linalg_iter
  unset tmp_linalg_vendor


# the minimum needed to declare linalg working...
sd_linalg_ok="no"
test "${sd_linalg_has_blas}" = "yes" -a "${sd_linalg_has_lapack}" = "yes" && sd_linalg_ok="yes"

# ---------------------------------------------------------------------------- #
# ---------------------------------------------------------------------------- #

#
# Optimized FFT support (must be done after linear algebra is fully configured)
#



  abi_sec_title="Optimized FFT support"

    abi_sec_spaces="                                                                      "
  abi_sec_tmp="${abi_sec_title}"
  while test "${abi_sec_tmp}" != ""; do
    abi_sec_spaces=`echo "${abi_sec_spaces}" | sed -e 's/^.//'`
    abi_sec_tmp=`echo "${abi_sec_tmp}" | sed -e 's/^.//'`
  done

  echo ""
  echo " =============================================================================="
  echo " === ${abi_sec_title}${abi_sec_spaces} ==="
  echo " =============================================================================="
  echo ""


if test "${sd_linalg_enable}" = "yes"; then

  # Init
  sd_fft_cppflags=""
  sd_fft_cflags=""
  sd_fft_cxxflags=""
  sd_fft_fcflags=""
  sd_fft_ldflags=""
  sd_fft_libs=""
  sd_fft_enable=""
  sd_fft_flavor=""
  sd_fft_init="unknown"
  sd_fft_ok="unknown"
  sd_fft_selected_flavors=""

  # Set adjustable parameters

  { $as_echo "$as_me:${as_lineno-$LINENO}: checking which FFT flavors to enable" >&5
$as_echo_n "checking which FFT flavors to enable... " >&6; }

  # Start from the internal implementation
  sd_fft_selected_flavors="goedecker"
  tmp_fft_has_fftw3=""
  tmp_linalg_has_mkl=`echo "${sd_linalg_flavor}" | grep "mkl"`

  # Prepend PFFT if available
  if test "${sd_pfft_init}" != "" -a "${sd_pfft_enable}" != "no"; then
    if test "${sd_mpi_ok}" = "yes"; then
      sd_fft_selected_flavors="pfft ${sd_fft_selected_flavors}"
    fi
  fi

  # Prepend FFTW3 if available
  if test "${sd_fftw3_init}" != "" -a "${sd_fftw3_enable}" != "no"; then
    if test "${tmp_linalg_has_mkl}" = ""; then
      sd_fft_selected_flavors="fftw3-threads fftw3 ${sd_fft_selected_flavors}"
    fi
  fi

  # Prepend DFTI if linear algebra is MKL
  if test "${tmp_linalg_has_mkl}" != ""; then
    sd_fft_selected_flavors="dfti ${sd_fft_selected_flavors}"
  fi

  { $as_echo "$as_me:${as_lineno-$LINENO}: result: ${sd_fft_selected_flavors}" >&5
$as_echo "${sd_fft_selected_flavors}" >&6; }

  # Warn about incompatibilities
  { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: MKL is incompatible with FFTW3

                    Please use DFTI instead and consult
                    https://software.intel.com/content/www/us/en/develop/documentation/onemkl-developer-reference-fortran/top/fourier-transform-functions.html

                    If you set the FFT flavor to fftw3, the configure script
                    will abort. Otherwise, your FFTW3 settings will be ignored.

" >&5
$as_echo "$as_me: WARNING: MKL is incompatible with FFTW3

                    Please use DFTI instead and consult
                    https://software.intel.com/content/www/us/en/develop/documentation/onemkl-developer-reference-fortran/top/fourier-transform-functions.html

                    If you set the FFT flavor to fftw3, the configure script
                    will abort. Otherwise, your FFTW3 settings will be ignored.

" >&2;}

  # Clean-up the mess
  unset tmp_fft_has_fftw3
  unset tmp_linalg_has_mkl


  # FIXME: policy and status are hard-coded for now
  sd_fft_policy="skip"
  sd_fft_status="optional"

  # Set reasonable defaults if not provided
  test -z "${sd_fft_policy}" && sd_fft_policy="fail"
  test -z "${sd_fft_status}" && sd_fft_status="optional"

  # Declare configure option

# Check whether --with-fft-flavor was given.
if test "${with_fft_flavor+set}" = set; then :
  withval=$with_fft_flavor;  tmp_fft_flavor_ok=`echo "${sd_fft_selected_flavors}" | grep "${withval}"`
      if test "${tmp_fft_flavor_ok}" = ""; then
        as_fn_error $? "invalid FFT flavor: '${withval}'" "$LINENO" 5
      fi
      sd_fft_flavor="${withval}"
      sd_fft_enable="yes"
      sd_fft_init="kwd"
      sd_fft_selected_flavors="${sd_fft_flavor}"
      unset tmp_fft_flavor_ok
else
   sd_fft_enable="unknown"; sd_fft_flavor="unknown"; sd_fft_init="def"
fi


  # Make sure configuration is correct
  if test "${STEREDEG_BYPASS_CONSISTENCY}" != "yes"; then

  # Check consistency between trigger value and package status
  tmp_fft_invalid="no"
  tmp_fft_flavor_chk=`echo "${sd_fft_choices}" | grep "${sd_fft_flavor}"`
  if test "${sd_fft_status}" = "implicit" -o \
          "${sd_fft_status}" = "required"; then
    if test "${tmp_fft_flavor_chk}" = ""; then
      case "${sd_fft_policy}" in
        fail)
          as_fn_error $? "invalid FFT flavor: sd_fft_flavor = '${sd_fft_flavor}'" "$LINENO" 5
          ;;
        skip)
          tmp_fft_invalid="yes"
          ;;
        warn)
          { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: The FFT package is required and cannot be disabled" >&5
$as_echo "$as_me: WARNING: The FFT package is required and cannot be disabled" >&2;}
          tmp_fft_invalid="yes"
          ;;
      esac
    fi
  fi

  # Fix wrong flavor value
  if test "${tmp_fft_invalid}" = "yes"; then
    sd_fft_flavor="${sd_fft_flavor_def}"
    { $as_echo "$as_me:${as_lineno-$LINENO}: setting sd_fft_flavor to '${sd_fft_flavor}'" >&5
$as_echo "$as_me: setting sd_fft_flavor to '${sd_fft_flavor}'" >&6;}
  fi

  # Implicit status overrides everything
  if test "${sd_fft_status}" = "implicit"; then
    if test "${sd_fft_cppflags}" != ""; then
      sd_fft_cppflags=""
      { $as_echo "$as_me:${as_lineno-$LINENO}: resetting FFT C preprocessing flags (implicit package)" >&5
$as_echo "$as_me: resetting FFT C preprocessing flags (implicit package)" >&6;}
    fi
    if test "${sd_fft_cflags}" != ""; then
      sd_fft_cflags=""
      { $as_echo "$as_me:${as_lineno-$LINENO}: resetting FFT C flags (implicit package)" >&5
$as_echo "$as_me: resetting FFT C flags (implicit package)" >&6;}
    fi
    if test "${sd_fft_fcflags}" != ""; then
      sd_fft_fcflags=""
      { $as_echo "$as_me:${as_lineno-$LINENO}: resetting FFT Fortran flags (implicit package)" >&5
$as_echo "$as_me: resetting FFT Fortran flags (implicit package)" >&6;}
    fi
    if test "${sd_fft_ldflags}" != ""; then
      sd_fft_ldflags=""
      { $as_echo "$as_me:${as_lineno-$LINENO}: resetting FFT linker flags (implicit package)" >&5
$as_echo "$as_me: resetting FFT linker flags (implicit package)" >&6;}
    fi
    if test "${sd_fft_libs}" != ""; then
      sd_fft_libs=""
      { $as_echo "$as_me:${as_lineno-$LINENO}: resetting FFT library flags (implicit package)" >&5
$as_echo "$as_me: resetting FFT library flags (implicit package)" >&6;}
    fi
  fi

  # Clean-up
  unset tmp_fft_flavor_chk
  unset tmp_fft_invalid

  fi

  # Display configuration

  { $as_echo "$as_me:${as_lineno-$LINENO}: checking for FFT flavor" >&5
$as_echo_n "checking for FFT flavor... " >&6; }
  { $as_echo "$as_me:${as_lineno-$LINENO}: result: ${sd_fft_flavor}" >&5
$as_echo "${sd_fft_flavor}" >&6; }
  { $as_echo "$as_me:${as_lineno-$LINENO}: checking for FFT C preprocessing flags" >&5
$as_echo_n "checking for FFT C preprocessing flags... " >&6; }
  { $as_echo "$as_me:${as_lineno-$LINENO}: result: ${sd_fft_cppflags}" >&5
$as_echo "${sd_fft_cppflags}" >&6; }
  { $as_echo "$as_me:${as_lineno-$LINENO}: checking for FFT C flags" >&5
$as_echo_n "checking for FFT C flags... " >&6; }
  { $as_echo "$as_me:${as_lineno-$LINENO}: result: ${sd_fft_cflags}" >&5
$as_echo "${sd_fft_cflags}" >&6; }
  { $as_echo "$as_me:${as_lineno-$LINENO}: checking for FFT Fortran flags" >&5
$as_echo_n "checking for FFT Fortran flags... " >&6; }
  { $as_echo "$as_me:${as_lineno-$LINENO}: result: ${sd_fft_fcflags}" >&5
$as_echo "${sd_fft_fcflags}" >&6; }
  { $as_echo "$as_me:${as_lineno-$LINENO}: checking for FFT linker flags" >&5
$as_echo_n "checking for FFT linker flags... " >&6; }
  { $as_echo "$as_me:${as_lineno-$LINENO}: result: ${sd_fft_ldflags}" >&5
$as_echo "${sd_fft_ldflags}" >&6; }
  { $as_echo "$as_me:${as_lineno-$LINENO}: checking for FFT library flags" >&5
$as_echo_n "checking for FFT library flags... " >&6; }
  { $as_echo "$as_me:${as_lineno-$LINENO}: result: ${sd_fft_libs}" >&5
$as_echo "${sd_fft_libs}" >&6; }


  # Export configuration















  for sd_fft_flavor in ${sd_fft_selected_flavors}; do

    { $as_echo "$as_me:${as_lineno-$LINENO}: checking for the FFT flavor to try" >&5
$as_echo_n "checking for the FFT flavor to try... " >&6; }
    { $as_echo "$as_me:${as_lineno-$LINENO}: result: ${sd_fft_flavor}" >&5
$as_echo "${sd_fft_flavor}" >&6; }
    case "${sd_fft_flavor}" in
      dfti)

  # Init
  sd_dfti_cppflags="${sd_linalg_cppflags}"
  sd_dfti_cflags="${sd_linalg_cflags}"
  sd_dfti_cxxflags="${sd_linalg_cxxflags}"
  sd_dfti_fcflags="${sd_linalg_fcflags}"
  sd_dfti_ldflags="${sd_linalg_ldflags}"
  sd_dfti_libs="${sd_linalg_libs}"
  sd_dfti_enable="yes"
  sd_dfti_init="mkl"
  sd_dfti_ok="unknown"

  # Display configuration

  { $as_echo "$as_me:${as_lineno-$LINENO}: checking whether to enable DFTI" >&5
$as_echo_n "checking whether to enable DFTI... " >&6; }
  { $as_echo "$as_me:${as_lineno-$LINENO}: result: ${sd_dfti_enable}" >&5
$as_echo "${sd_dfti_enable}" >&6; }
  if test "${sd_dfti_enable}" != "no"; then
    { $as_echo "$as_me:${as_lineno-$LINENO}: checking how DFTI parameters have been set" >&5
$as_echo_n "checking how DFTI parameters have been set... " >&6; }
    { $as_echo "$as_me:${as_lineno-$LINENO}: result: ${sd_dfti_init}" >&5
$as_echo "${sd_dfti_init}" >&6; }
    { $as_echo "$as_me:${as_lineno-$LINENO}: checking for DFTI C preprocessing flags" >&5
$as_echo_n "checking for DFTI C preprocessing flags... " >&6; }
    if test "${sd_dfti_cppflags}" = ""; then
      { $as_echo "$as_me:${as_lineno-$LINENO}: result: none" >&5
$as_echo "none" >&6; }
    else
      { $as_echo "$as_me:${as_lineno-$LINENO}: result: ${sd_dfti_cppflags}" >&5
$as_echo "${sd_dfti_cppflags}" >&6; }
    fi
    { $as_echo "$as_me:${as_lineno-$LINENO}: checking for DFTI C flags" >&5
$as_echo_n "checking for DFTI C flags... " >&6; }
    if test "${sd_dfti_cflags}" = ""; then
      { $as_echo "$as_me:${as_lineno-$LINENO}: result: none" >&5
$as_echo "none" >&6; }
    else
      { $as_echo "$as_me:${as_lineno-$LINENO}: result: ${sd_dfti_cflags}" >&5
$as_echo "${sd_dfti_cflags}" >&6; }
    fi
    { $as_echo "$as_me:${as_lineno-$LINENO}: checking for DFTI Fortran flags" >&5
$as_echo_n "checking for DFTI Fortran flags... " >&6; }
    if test "${sd_dfti_fcflags}" = ""; then
      { $as_echo "$as_me:${as_lineno-$LINENO}: result: none" >&5
$as_echo "none" >&6; }
    else
      { $as_echo "$as_me:${as_lineno-$LINENO}: result: ${sd_dfti_fcflags}" >&5
$as_echo "${sd_dfti_fcflags}" >&6; }
    fi
    { $as_echo "$as_me:${as_lineno-$LINENO}: checking for DFTI linker flags" >&5
$as_echo_n "checking for DFTI linker flags... " >&6; }
    if test "${sd_dfti_ldflags}" = ""; then
      { $as_echo "$as_me:${as_lineno-$LINENO}: result: none" >&5
$as_echo "none" >&6; }
    else
      { $as_echo "$as_me:${as_lineno-$LINENO}: result: ${sd_dfti_ldflags}" >&5
$as_echo "${sd_dfti_ldflags}" >&6; }
    fi
    { $as_echo "$as_me:${as_lineno-$LINENO}: checking for DFTI library flags" >&5
$as_echo_n "checking for DFTI library flags... " >&6; }
    if test "${sd_dfti_libs}" = ""; then
      { $as_echo "$as_me:${as_lineno-$LINENO}: result: none" >&5
$as_echo "none" >&6; }
    else
      { $as_echo "$as_me:${as_lineno-$LINENO}: result: ${sd_dfti_libs}" >&5
$as_echo "${sd_dfti_libs}" >&6; }
    fi
  fi


  # Check whether we can compile and link a simple program
  # and update build flags if successful
  if test "${sd_dfti_enable}" = "auto" -o "${sd_dfti_enable}" = "yes"; then

  # Prepare environment

  sd_esl_saved_CPPFLAGS="${CPPFLAGS}"
  sd_esl_saved_CFLAGS="${CFLAGS}"
  sd_esl_saved_CXXFLAGS="${CXXFLAGS}"
  sd_esl_saved_FCFLAGS="${FCFLAGS}"
  sd_esl_saved_LDFLAGS="${LDFLAGS}"
  sd_esl_saved_LIBS="${LIBS}"

  if test "${sd_dfti_init}" = "esl"; then
    { $as_echo "$as_me:${as_lineno-$LINENO}: will look for DFTI in the installed ESL Bundle" >&5
$as_echo "$as_me: will look for DFTI in the installed ESL Bundle" >&6;}

  CPPFLAGS="${CPPFLAGS} ${sd_esl_bundle_cppflags}"
  CFLAGS="${CFLAGS} ${sd_esl_bundle_cflags}"
  CXXFLAGS="${CXXFLAGS} ${sd_esl_bundle_cxxflags}"
  FCFLAGS="${FCFLAGS} ${sd_esl_bundle_fcflags}"
  LDFLAGS="${LDFLAGS} ${sd_esl_bundle_ldflags}"
  if test ! -z ""; then
    CPPFLAGS="${CPPFLAGS} -I${ESL_BUNDLE_PREFIX}/include/"
    FCFLAGS="${FCFLAGS} -I${ESL_BUNDLE_PREFIX}/include/"
  fi
  FCFLAGS="${FCFLAGS} -I/usr/include"


  LIBS="${sd_esl_bundle_libdirs} ${sd_dfti_libs_def} ${sd_esl_bundle_libs} ${LIBS}"

  else
    CPPFLAGS="${CPPFLAGS} ${sd_dfti_cppflags}"
    CFLAGS="${CFLAGS} ${sd_dfti_cflags}"
    FCFLAGS="${FCFLAGS} ${sd_dfti_fcflags}"
    LDFLAGS="${LDFLAGS} ${sd_dfti_ldflags}"
    LIBS="${sd_dfti_libs} ${LIBS}"
  fi

  # Check DFTI C API
  # FIXME: Very complex to have it work properly, would need a replacement
  #        of AC_LINK_IFELSE accepting prologues, because of the included
  #        mkl_dfti.f90 file.
  { $as_echo "$as_me:${as_lineno-$LINENO}: checking whether the DFTI library works" >&5
$as_echo_n "checking whether the DFTI library works... " >&6; }
  sd_dfti_ok="yes"
  { $as_echo "$as_me:${as_lineno-$LINENO}: result: ${sd_dfti_ok}" >&5
$as_echo "${sd_dfti_ok}" >&6; }

  # Restore environment

  CPPFLAGS="${sd_esl_saved_CPPFLAGS}"
  CFLAGS="${sd_esl_saved_CFLAGS}"
  CXXFLAGS="${sd_esl_saved_CXXFLAGS}"
  FCFLAGS="${sd_esl_saved_FCFLAGS}"
  LDFLAGS="${sd_esl_saved_LDFLAGS}"
  LIBS="${sd_esl_saved_LIBS}"



    if test "${sd_dfti_ok}" = "yes"; then
      if test "${sd_dfti_init}" = "esl"; then
        sd_esl_bundle_libs="${sd_dfti_libs_def} ${sd_esl_bundle_libs}"
      else
        LIBS="${sd_dfti_libs} ${LIBS}"
      fi
      LDFLAGS="${LDFLAGS} ${sd_dfti_ldflags}"


$as_echo "#define HAVE_DFTI 1" >>confdefs.h

    else
      if test "${sd_dfti_status}" = "optional" -a \
              "${sd_dfti_init}" = "def"; then
        sd_dfti_enable="no"
        sd_dfti_cppflags=""
        sd_dfti_cflags=""
        sd_dfti_fcflags=""
        sd_dfti_ldflags=""
        sd_dfti_libs=""
      else
        { { $as_echo "$as_me:${as_lineno-$LINENO}: error: in \`$ac_pwd':" >&5
$as_echo "$as_me: error: in \`$ac_pwd':" >&2;}
as_fn_error $? "invalid DFTI configuration
See \`config.log' for more details" "$LINENO" 5; }
      fi
    fi
  else
    sd_dfti_enable="no"
    sd_dfti_cppflags=""
    sd_dfti_cflags=""
    sd_dfti_fcflags=""
    sd_dfti_ldflags=""
    sd_dfti_libs=""
  fi

        if test "${sd_dfti_ok}" = "yes"; then
          sd_fft_cppflags="${sd_dfti_cppflags}"
          sd_fft_cflags="${sd_dfti_cflags}"
          sd_fft_fcflags="${sd_dfti_fcflags}"
          sd_fft_ldflags="${sd_dfti_ldflags}"
          sd_fft_libs="${sd_dfti_libs}"
          sd_fft_ok="yes"
        fi
        ;;
      fftw3)

  # Display configuration

  { $as_echo "$as_me:${as_lineno-$LINENO}: checking whether to enable FFTW3" >&5
$as_echo_n "checking whether to enable FFTW3... " >&6; }
  { $as_echo "$as_me:${as_lineno-$LINENO}: result: ${sd_fftw3_enable}" >&5
$as_echo "${sd_fftw3_enable}" >&6; }
  if test "${sd_fftw3_enable}" != "no"; then
    { $as_echo "$as_me:${as_lineno-$LINENO}: checking how FFTW3 parameters have been set" >&5
$as_echo_n "checking how FFTW3 parameters have been set... " >&6; }
    { $as_echo "$as_me:${as_lineno-$LINENO}: result: ${sd_fftw3_init}" >&5
$as_echo "${sd_fftw3_init}" >&6; }
    { $as_echo "$as_me:${as_lineno-$LINENO}: checking for FFTW3 C preprocessing flags" >&5
$as_echo_n "checking for FFTW3 C preprocessing flags... " >&6; }
    if test "${sd_fftw3_cppflags}" = ""; then
      { $as_echo "$as_me:${as_lineno-$LINENO}: result: none" >&5
$as_echo "none" >&6; }
    else
      { $as_echo "$as_me:${as_lineno-$LINENO}: result: ${sd_fftw3_cppflags}" >&5
$as_echo "${sd_fftw3_cppflags}" >&6; }
    fi
    { $as_echo "$as_me:${as_lineno-$LINENO}: checking for FFTW3 C flags" >&5
$as_echo_n "checking for FFTW3 C flags... " >&6; }
    if test "${sd_fftw3_cflags}" = ""; then
      { $as_echo "$as_me:${as_lineno-$LINENO}: result: none" >&5
$as_echo "none" >&6; }
    else
      { $as_echo "$as_me:${as_lineno-$LINENO}: result: ${sd_fftw3_cflags}" >&5
$as_echo "${sd_fftw3_cflags}" >&6; }
    fi
    { $as_echo "$as_me:${as_lineno-$LINENO}: checking for FFTW3 Fortran flags" >&5
$as_echo_n "checking for FFTW3 Fortran flags... " >&6; }
    if test "${sd_fftw3_fcflags}" = ""; then
      { $as_echo "$as_me:${as_lineno-$LINENO}: result: none" >&5
$as_echo "none" >&6; }
    else
      { $as_echo "$as_me:${as_lineno-$LINENO}: result: ${sd_fftw3_fcflags}" >&5
$as_echo "${sd_fftw3_fcflags}" >&6; }
    fi
    { $as_echo "$as_me:${as_lineno-$LINENO}: checking for FFTW3 linker flags" >&5
$as_echo_n "checking for FFTW3 linker flags... " >&6; }
    if test "${sd_fftw3_ldflags}" = ""; then
      { $as_echo "$as_me:${as_lineno-$LINENO}: result: none" >&5
$as_echo "none" >&6; }
    else
      { $as_echo "$as_me:${as_lineno-$LINENO}: result: ${sd_fftw3_ldflags}" >&5
$as_echo "${sd_fftw3_ldflags}" >&6; }
    fi
    { $as_echo "$as_me:${as_lineno-$LINENO}: checking for FFTW3 library flags" >&5
$as_echo_n "checking for FFTW3 library flags... " >&6; }
    if test "${sd_fftw3_libs}" = ""; then
      { $as_echo "$as_me:${as_lineno-$LINENO}: result: none" >&5
$as_echo "none" >&6; }
    else
      { $as_echo "$as_me:${as_lineno-$LINENO}: result: ${sd_fftw3_libs}" >&5
$as_echo "${sd_fftw3_libs}" >&6; }
    fi
  fi


  # Check whether we can compile and link a simple program
  # and update build flags if successful
  if test "${sd_fftw3_enable}" = "auto" -o "${sd_fftw3_enable}" = "yes"; then

  # Prepare environment

  sd_esl_saved_CPPFLAGS="${CPPFLAGS}"
  sd_esl_saved_CFLAGS="${CFLAGS}"
  sd_esl_saved_CXXFLAGS="${CXXFLAGS}"
  sd_esl_saved_FCFLAGS="${FCFLAGS}"
  sd_esl_saved_LDFLAGS="${LDFLAGS}"
  sd_esl_saved_LIBS="${LIBS}"

  if test "${sd_fftw3_init}" = "esl"; then
    { $as_echo "$as_me:${as_lineno-$LINENO}: will look for FFTW3 in the installed ESL Bundle" >&5
$as_echo "$as_me: will look for FFTW3 in the installed ESL Bundle" >&6;}

  CPPFLAGS="${CPPFLAGS} ${sd_esl_bundle_cppflags}"
  CFLAGS="${CFLAGS} ${sd_esl_bundle_cflags}"
  CXXFLAGS="${CXXFLAGS} ${sd_esl_bundle_cxxflags}"
  FCFLAGS="${FCFLAGS} ${sd_esl_bundle_fcflags}"
  LDFLAGS="${LDFLAGS} ${sd_esl_bundle_ldflags}"
  if test ! -z ""; then
    CPPFLAGS="${CPPFLAGS} -I${ESL_BUNDLE_PREFIX}/include/"
    FCFLAGS="${FCFLAGS} -I${ESL_BUNDLE_PREFIX}/include/"
  fi
  FCFLAGS="${FCFLAGS} -I/usr/include"


  LIBS="${sd_esl_bundle_libdirs} ${sd_fftw3_libs_def} ${sd_esl_bundle_libs} ${LIBS}"

  else
    CPPFLAGS="${CPPFLAGS} ${sd_fftw3_cppflags}"
    CFLAGS="${CFLAGS} ${sd_fftw3_cflags}"
    FCFLAGS="${FCFLAGS} ${sd_fftw3_fcflags}"
    LDFLAGS="${LDFLAGS} ${sd_fftw3_ldflags}"
    LIBS="${sd_fftw3_libs} ${LIBS}"
  fi

  # Check FFTW3 C API
  { $as_echo "$as_me:${as_lineno-$LINENO}: checking whether the FFTW3 library works" >&5
$as_echo_n "checking whether the FFTW3 library works... " >&6; }
  ac_ext=c
ac_cpp='$CPP $CPPFLAGS'
ac_compile='$CC -c $CFLAGS $CPPFLAGS conftest.$ac_ext >&5'
ac_link='$CC -o conftest$ac_exeext $CFLAGS $CPPFLAGS $LDFLAGS conftest.$ac_ext $LIBS >&5'
ac_compiler_gnu=$ac_cv_c_compiler_gnu

  cat confdefs.h - <<_ACEOF >conftest.$ac_ext
/* end confdefs.h.  */

#     include <fftw3.h>

#ifdef FC_DUMMY_MAIN
#ifndef FC_DUMMY_MAIN_EQ_F77
#  ifdef __cplusplus
     extern "C"
#  endif
   int FC_DUMMY_MAIN() { return 1; }
#endif
#endif
int
main ()
{

      fftw_plan *plan;
      fftw_complex *a1, *a2;
      fftw_execute_dft(*plan, a1, a2);

  ;
  return 0;
}
