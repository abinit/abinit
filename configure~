$as_echo "$as_me: triggering the 'DO_BUILD_EXPORTS' conditional" >&6;}
  fi
   if test "${abi_exports_enable}" = "yes"; then
  DO_BUILD_EXPORTS_TRUE=
  DO_BUILD_EXPORTS_FALSE='#'
else
  DO_BUILD_EXPORTS_TRUE='#'
  DO_BUILD_EXPORTS_FALSE=
fi


  # Triggers for enable_gw_dpc
