$as_echo "$as_me:   * enable_mpi_inplace = '${enable_mpi_inplace}'" >&6;}
  { $as_echo "$as_me:${as_lineno-$LINENO}:   * enable_mpi_io      = '${enable_mpi_io}'" >&5
$as_echo "$as_me:   * enable_mpi_io      = '${enable_mpi_io}'" >&6;}
