#ifndef FC_DUMMY_MAIN_EQ_F77
#  ifdef __cplusplus
     extern "C"
#  endif
   int FC_DUMMY_MAIN() { return 1; }
#endif
#endif
int
main ()
{
return logl ();
  ;
  return 0;
}
_ACEOF
for ac_lib in '' m ml; do
  if test -z "$ac_lib"; then
    ac_res="none required"
  else
