    abi_env_CXXFLAGS="${CXXFLAGS}"

    abi_env_CXXFLAGS_DEBUG="${CXXFLAGS_DEBUG}"

    abi_env_CXXFLAGS_EXTRA="${CXXFLAGS_EXTRA}"

    abi_env_CXXFLAGS_GPU="${CXXFLAGS_GPU}"

