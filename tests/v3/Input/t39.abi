# Common data
prtvol -1          # stop in abinit (main program), before call driver.
brvltt -1          # important to reduce the conventional cell to primitive,
                   # and allow the recognition of space group
acell 8 8 8
ntypat 1
znucl 1
natrd 1
xred 0.17 0.15 0.11
spinat 0.0 0.0 1.0
typat 1
ecut 1
  kptopt 0
nkpt 1
nline 1
nstep 1
toldfe 1.0d-6

 pp_dirpath "$ABI_PSPDIR"
 pseudos "01h.bare"

