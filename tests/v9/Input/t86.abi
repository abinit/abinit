spin_ntime =1000
spin_nctime=10
spin_dt= 2.41888e-16 s #e-15 s
spin_projection_qpoint = 0.5 0.5 0.5
spin_write_traj=1
spin_damping=0.0

slc_coupling = 0010 # activate only Oiju terms


randomseed=111111 # for testing only



