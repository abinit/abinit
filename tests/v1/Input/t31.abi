#   Sn in Box.

 acell 3*12
 diemac 1.0d0
 diemix 0.333333333333d0
 ecut 14
 enunit 2
 intxc 1
 kptopt 0
 kpt   0.25 0.25 0.25   kptnrm 1
 natom  1 nband 4
 nkpt  1
 nstep 8
 nsym  8 ntypat  1
 occopt 0 occ 1*2.0d0 3*0.66666666666666666666666d0
 prtvol 10
 rprim  1 0 0 0 1 0 0 0 1
 symrel
       1  0  0    0  1  0    0  0  1
      -1  0  0    0  1  0    0  0  1
       1  0  0    0 -1  0    0  0  1
       1  0  0    0  1  0    0  0 -1
      -1  0  0    0 -1  0    0  0  1
      -1  0  0    0  1  0    0  0 -1
       1  0  0    0 -1  0    0  0 -1
      -1  0  0    0 -1  0    0  0 -1
 tnons  24*0d0
 tolwfr 1.0d-16
 typat  1
 wtk   1
 xred  0.5 0.5 0.5
 znucl 50


 pp_dirpath "$ABI_PSPDIR"
 pseudos "PseudosTM_pwteter/50sn.pspnc"

