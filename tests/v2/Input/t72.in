# hR lattice of magnesium atoms
# Examine the list of k points

 prtkpt 1
 kptopt 1
 kptrlen 30.0d0

 acell 3*6.0d0
 angdeg 3*70.0d0

 ecut 4.5
 natom 1
 nstep 1
 ntypat  1
 tolvrs 3.0d-20
 typat  1
 xcart 0.0 0.0 0.0
 znucl 12.0

 pp_dirpath "$ABI_PSPDIR"
 pseudos "PseudosTM_pwteter/12mg.pspnc"

